{"ast":null,"code":"import Layout from \"../../layouts/main.vue\";\nimport PageHeader from \"@/components/page-header\";\nimport 'prismjs';\nimport 'prismjs/themes/prism.css';\nexport default {\n  data() {\n    return {\n      title: \"Images\",\n      items: [{\n        text: \"Base UI\",\n        href: \"/\"\n      }, {\n        text: \"Images\",\n        active: true\n      }]\n    };\n  },\n  name: \"Images\",\n  components: {\n    Layout,\n    PageHeader\n  },\n  mounted() {\n    const checkbox = document.getElementsByClassName(\"code-switcher\");\n    Array.from(checkbox).forEach(check => {\n      check.addEventListener('change', () => {\n        const card = check.closest('.card');\n        const preview = card.querySelector(\".live-preview\");\n        const code = card.querySelector(\".code-view\");\n        if (check.checked) {\n          // do this\n          preview.classList.add(\"d-none\");\n          code.classList.remove(\"d-none\");\n        } else {\n          // do that\n          preview.classList.remove(\"d-none\");\n          code.classList.add(\"d-none\");\n        }\n      });\n    });\n  }\n};","map":{"version":3,"names":["Layout","PageHeader","data","title","items","text","href","active","name","components","mounted","checkbox","document","getElementsByClassName","Array","from","forEach","check","addEventListener","card","closest","preview","querySelector","code","checked","classList","add","remove"],"sources":["C:\\Users\\UMIDJON\\Desktop\\default2\\src\\views\\ui\\images.vue"],"sourcesContent":["<script>\nimport Layout from \"../../layouts/main.vue\";\nimport PageHeader from \"@/components/page-header\";\n\nimport 'prismjs';\nimport 'prismjs/themes/prism.css';\n\nexport default {\n    data() {\n        return {\n            title: \"Images\",\n            items: [\n                {\n                    text: \"Base UI\",\n                    href: \"/\",\n                },\n                {\n                    text: \"Images\",\n                    active: true,\n                },\n            ],\n        };\n    },\n    name: \"Images\",\n    components: {\n        Layout,\n        PageHeader,\n    },\n    mounted() {\n        const checkbox = document.getElementsByClassName(\"code-switcher\");\n        Array.from(checkbox).forEach((check) => {\n            check.addEventListener('change', () => {\n                const card = check.closest('.card');\n                const preview = card.querySelector(\".live-preview\");\n                const code = card.querySelector(\".code-view\");\n                if (check.checked) {\n                    // do this\n                    preview.classList.add(\"d-none\");\n                    code.classList.remove(\"d-none\");\n                } else {\n                    // do that\n                    preview.classList.remove(\"d-none\");\n                    code.classList.add(\"d-none\");\n                }\n            });\n        });\n    }\n};\n</script>\n\n<template>\n    <Layout>\n        <PageHeader :title=\"title\" :items=\"items\" />\n        <b-row>\n            <b-col xxl=\"6\">\n                <b-card no-body>\n                    <b-card-header class=\"align-items-center d-flex\">\n                        <b-card-title class=\"mb-0 flex-grow-1\">Image Rounded & Circle</b-card-title>\n                        <div class=\"flex-shrink-0\">\n                            <div class=\"form-check form-switch form-switch-right form-switch-md\">\n                                <label for=\"rounded-circle-image\" class=\"form-label text-muted\">Show Code</label>\n                                <input class=\"form-check-input code-switcher\" type=\"checkbox\"\n                                    id=\"rounded-circle-image\" />\n                            </div>\n                        </div>\n                    </b-card-header>\n\n                    <b-card-body>\n                        <p class=\"text-muted\">\n                            Use\n                            <code>b-avatar rounded=\"sm\"</code> to show an image with a round border and rounded shape\n                            respectively.\n                        </p>\n\n                        <div class=\"live-preview\">\n                            <b-row class=\"align-items-center\">\n                                <b-col cols=\"6\">\n                                    <img class=\"rounded\" alt=\"200x200\" width=\"200\" src=\"@/assets/images/small/img-4.jpg\"\n                                        data-holder-rendered=\"true\" />\n                                </b-col>\n                                <b-col cols=\"6\">\n                                    <div class=\"mt-4 mt-md-0\">\n                                        <img class=\"rounded-circle avatar-xl\" alt=\"200x200\"\n                                            src=\"@/assets/images/users/avatar-4.jpg\" data-holder-rendered=\"true\" />\n                                    </div>\n                                </b-col>\n                            </b-row>\n                        </div>\n\n                        <div class=\"d-none code-view\">\n                            <pre class=\"language-markup\">\n<code>&lt;!-- Rounded Image --&gt;\n&lt;b-img class=&quot;rounded&quot; alt=&quot;200x200&quot; width=&quot;200&quot; src=&quot;...&quot; data-holder-rendered=&quot;true&quot;&gt;&lt;/b-img&gt;</code>\n\n<code>&lt;!-- Rounded-circle Image --&gt;\n&lt;b-img class=&quot;rounded-circle avatar-xl&quot; alt=&quot;200x200&quot; src=&quot;...&quot; data-holder-rendered=&quot;true&quot;&gt;&lt;/b-img&gt;</code></pre>\n                        </div>\n                    </b-card-body>\n                </b-card>\n            </b-col>\n\n            <b-col xxl=\"6\">\n                <b-card no-body>\n                    <b-card-header class=\"align-items-center d-flex\">\n                        <b-card-title class=\"mb-0 flex-grow-1\">Image Thumbnails</b-card-title>\n                        <div class=\"flex-shrink-0\">\n                            <div class=\"form-check form-switch form-switch-right form-switch-md\">\n                                <label for=\"thumbnails-image\" class=\"form-label text-muted\">Show Code</label>\n                                <input class=\"form-check-input code-switcher\" type=\"checkbox\" id=\"thumbnails-image\" />\n                            </div>\n                        </div>\n                    </b-card-header>\n\n                    <b-card-body>\n                        <p class=\"text-muted\">\n                            Use\n                            <code>img-thumbnail</code> class to give an image\n                            rounded 1px border appearance.\n                        </p>\n\n                        <div class=\"live-preview\">\n                            <b-row>\n                                <b-col cols=\"6\">\n                                    <img class=\"img-thumbnail\" alt=\"200x200\" width=\"200\"\n                                        src=\"@/assets/images/small/img-3.jpg\" data-holder-rendered=\"true\" />\n                                </b-col>\n                                <b-col cols=\"6\">\n                                    <div class=\"mt-4 mt-md-0\">\n                                        <img class=\"img-thumbnail rounded-circle avatar-xl\" alt=\"200x200\"\n                                            src=\"@/assets/images/users/avatar-3.jpg\" data-holder-rendered=\"true\" />\n                                    </div>\n                                </b-col>\n                            </b-row>\n                        </div>\n\n                        <div class=\"d-none code-view\">\n                            <pre class=\"language-markup\">\n<code>&lt;!-- Thumbnails Images --&gt;\n&lt;b-img class=&quot;img-thumbnail&quot; alt=&quot;200x200&quot; width=&quot;200&quot; src=&quot;...&quot; data-holder-rendered=&quot;true&quot;&gt;&lt;/b-img&gt;</code>\n\n<code>&lt;b-img class=&quot;img-thumbnail rounded-circle avatar-xl&quot; alt=&quot;200x200&quot; src=&quot;...&quot; data-holder-rendered=&quot;true&quot;&gt;&lt;/b-img&gt;</code></pre>\n                        </div>\n                    </b-card-body>\n                </b-card>\n            </b-col>\n        </b-row>\n\n        <b-row>\n            <b-col lg=\"12\">\n                <b-card no-body>\n                    <b-card-header class=\"align-items-center d-flex\">\n                        <b-card-title class=\"mb-0 flex-grow-1\">Image Sizes</b-card-title>\n                        <div class=\"flex-shrink-0\">\n                            <div class=\"form-check form-switch form-switch-right form-switch-md\">\n                                <label for=\"image-size\" class=\"form-label text-muted\">Show Code</label>\n                                <input class=\"form-check-input code-switcher\" type=\"checkbox\" id=\"image-size\" />\n                            </div>\n                        </div>\n                    </b-card-header>\n\n                    <b-card-body>\n                        <p class=\"text-muted mb-4\">\n                            Use\n                            <code>avatar-xxs</code>,\n                            <code>avatar-xs</code>,\n                            <code>avatar-sm</code>,\n                            <code>avatar-md</code>,\n                            <code>avatar-lg</code>,\n                            <code>avatar-xl</code> class for different image sizes.\n                        </p>\n\n                        <div class=\"live-preview\">\n                            <b-row>\n                                <b-col md=\"12\">\n                                    <b-row class=\"g-3\">\n                                        <b-col xxl=\"2\" md=\"4\" cols=\"6\">\n                                            <div>\n                                                <img src=\"@/assets/images/users/avatar-2.jpg\" alt\n                                                    class=\"rounded avatar-xxs\" />\n                                                <p class=\"mt-2 mb-lg-0\">\n                                                    <code>avatar-xxs</code>\n                                                </p>\n                                            </div>\n                                        </b-col>\n                                        <b-col xxl=\"2\" md=\"4\" cols=\"6\">\n                                            <div>\n                                                <img src=\"@/assets/images/users/avatar-10.jpg\" alt\n                                                    class=\"rounded avatar-xs\" />\n                                                <p class=\"mt-2 mb-lg-0\">\n                                                    <code>avatar-xs</code>\n                                                </p>\n                                            </div>\n                                        </b-col>\n                                        <b-col xxl=\"2\" md=\"4\" cols=\"6\">\n                                            <div>\n                                                <img src=\"@/assets/images/users/avatar-3.jpg\" alt\n                                                    class=\"rounded avatar-sm\" />\n                                                <p class=\"mt-2 mb-lg-0\">\n                                                    <code>avatar-sm</code>\n                                                </p>\n                                            </div>\n                                        </b-col>\n                                        <b-col xxl=\"2\" md=\"4\" cols=\"6\">\n                                            <div>\n                                                <img src=\"@/assets/images/users/avatar-4.jpg\" alt\n                                                    class=\"rounded avatar-md\" />\n                                                <p class=\"mt-2 mb-lg-0\">\n                                                    <code>avatar-md</code>\n                                                </p>\n                                            </div>\n                                        </b-col>\n                                        <b-col xxl=\"2\" md=\"4\" cols=\"6\">\n                                            <div>\n                                                <img src=\"@/assets/images/users/avatar-5.jpg\" alt\n                                                    class=\"rounded avatar-lg\" />\n                                                <p class=\"mt-2 mb-lg-0\">\n                                                    <code>avatar-lg</code>\n                                                </p>\n                                            </div>\n                                        </b-col>\n                                        <b-col xxl=\"2\" md=\"4\" cols=\"6\">\n                                            <div>\n                                                <img src=\"@/assets/images/users/avatar-8.jpg\" alt\n                                                    class=\"rounded avatar-xl\" />\n                                                <p class=\"mt-2 mb-lg-0\">\n                                                    <code>avatar-xl</code>\n                                                </p>\n                                            </div>\n                                        </b-col>\n                                    </b-row>\n                                </b-col>\n\n                                <b-col md=\"12\">\n                                    <b-row class=\"g-3 mt-5\">\n                                        <b-col xxl=\"2\" md=\"4\" cols=\"6\">\n                                            <div>\n                                                <img src=\"@/assets/images/users/avatar-2.jpg\" alt\n                                                    class=\"rounded-circle avatar-xxs\" />\n                                                <p class=\"mt-2 mb-lg-0\">\n                                                    <code>avatar-xxs</code>\n                                                </p>\n                                            </div>\n                                        </b-col>\n                                        <b-col xxl=\"2\" md=\"4\" cols=\"6\">\n                                            <div>\n                                                <img src=\"@/assets/images/users/avatar-10.jpg\" alt\n                                                    class=\"rounded-circle avatar-xs\" />\n                                                <p class=\"mt-2 mb-lg-0\">\n                                                    <code>avatar-xs</code>\n                                                </p>\n                                            </div>\n                                        </b-col>\n                                        <b-col xxl=\"2\" md=\"4\" cols=\"6\">\n                                            <div>\n                                                <img src=\"@/assets/images/users/avatar-3.jpg\" alt\n                                                    class=\"rounded-circle avatar-sm\" />\n                                                <p class=\"mt-2 mb-lg-0\">\n                                                    <code>avatar-sm</code>\n                                                </p>\n                                            </div>\n                                        </b-col>\n                                        <b-col xxl=\"2\" md=\"4\" cols=\"6\">\n                                            <div>\n                                                <img src=\"@/assets/images/users/avatar-4.jpg\" alt\n                                                    class=\"rounded-circle avatar-md\" />\n                                                <p class=\"mt-2 mb-lg-0\">\n                                                    <code>avatar-md</code>\n                                                </p>\n                                            </div>\n                                        </b-col>\n                                        <b-col xxl=\"2\" md=\"4\" cols=\"6\">\n                                            <div>\n                                                <img src=\"@/assets/images/users/avatar-5.jpg\" alt\n                                                    class=\"rounded-circle avatar-lg\" />\n                                                <p class=\"mt-2 mb-lg-0\">\n                                                    <code>avatar-lg</code>\n                                                </p>\n                                            </div>\n                                        </b-col>\n                                        <b-col xxl=\"2\" md=\"4\" cols=\"6\">\n                                            <div>\n                                                <img src=\"@/assets/images/users/avatar-8.jpg\" alt\n                                                    class=\"rounded-circle avatar-xl\" />\n                                                <p class=\"mt-2 mb-lg-0\">\n                                                    <code>avatar-xl</code>\n                                                </p>\n                                            </div>\n                                        </b-col>\n                                    </b-row>\n                                </b-col>\n                            </b-row>\n                        </div>\n\n                        <div class=\"d-none code-view\">\n                            <pre class=\"language-markup\"\n                                style=\"height: 275px;\"><code>&lt;!-- Image Sizes --&gt;\n&lt;b-img src=&quot;...&quot; alt=&quot;&quot; class=&quot;rounded avatar-xxs&quot;/&gt;</code>\n\n<code>&lt;b-img src=&quot;...&quot; alt=&quot;&quot; class=&quot;rounded avatar-xs&quot;&gt;&lt;/b-img&gt;</code>\n\n<code>&lt;b-img src=&quot;...&quot; alt=&quot;&quot; class=&quot;rounded avatar-sm&quot;&gt;&lt;/b-img&gt;</code>\n\n<code>&lt;b-img src=&quot;...&quot; alt=&quot;&quot; class=&quot;rounded avatar-md&quot;&gt;&lt;/b-img&gt;</code>\n\n<code>&lt;b-img src=&quot;...&quot; alt=&quot;&quot; class=&quot;rounded avatar-lg&quot;&gt;&lt;/b-img&gt;</code>\n\n<code>&lt;b-img src=&quot;...&quot; alt=&quot;&quot; class=&quot;rounded avatar-xl&quot;&gt;&lt;/b-img&gt;</code>\n\n<code>&lt;b-img src=&quot;...&quot; alt=&quot;&quot; class=&quot;rounded-circle avatar-xxs&quot;&gt;&lt;/b-img&gt;</code>\n\n<code>&lt;b-img src=&quot;...&quot; alt=&quot;&quot; class=&quot;rounded-circle avatar-xs&quot;&gt;&lt;/b-img&gt;</code>\n\n<code>&lt;b-img src=&quot;...&quot; alt=&quot;&quot; class=&quot;rounded-circle avatar-sm&quot;&gt;&lt;/b-img&gt;</code>\n\n<code>&lt;b-img src=&quot;...&quot; alt=&quot;&quot; class=&quot;rounded-circle avatar-md&quot;&gt;&lt;/b-img&gt;</code>\n\n<code>&lt;b-img src=&quot;...&quot; alt=&quot;&quot; class=&quot;rounded-circle avatar-lg&quot;&gt;&lt;/b-img&gt;</code>\n\n<code>&lt;b-img src=&quot;...&quot; alt=&quot;&quot; class=&quot;rounded-circle avatar-xl&quot;&gt;&lt;/b-img&gt;</code></pre>\n                        </div>\n                    </b-card-body>\n                </b-card>\n            </b-col>\n        </b-row>\n\n        <b-row>\n            <b-col lg=\"12\">\n                <b-card no-body>\n                    <b-card-header class=\"align-items-center d-flex\">\n                        <b-card-title class=\"mb-0 flex-grow-1\">Avatar With Content</b-card-title>\n                        <div class=\"flex-shrink-0\">\n                            <div class=\"form-check form-switch form-switch-right form-switch-md\">\n                                <label for=\"avatar-content\" class=\"form-label text-muted\">Show Code</label>\n                                <input class=\"form-check-input code-switcher\" type=\"checkbox\" id=\"avatar-content\" />\n                            </div>\n                        </div>\n                    </b-card-header>\n\n                    <b-card-body>\n                        <p class=\"text-muted\">\n                            Use\n                            <code>avatar-xxs, avatar-xs,avatar-sm,avatar-md,avatar-lg,avatar-xl</code> class to set\n                            different avatar content.\n                        </p>\n\n                        <div class=\"live-preview\">\n                            <b-row class=\"g-3\">\n                                <b-col xxl=\"2\" md=\"4\" cols=\"6\">\n                                    <div class=\"avatar-xxs mt-3\">\n                                        <div class=\"avatar-title rounded bg-soft-primary text-primary fs-10\">XXS</div>\n                                    </div>\n                                </b-col>\n                                <b-col xxl=\"2\" md=\"4\" cols=\"6\">\n                                    <div class=\"avatar-xs mt-3\">\n                                        <div class=\"avatar-title rounded bg-soft-secondary text-secondary\">XS</div>\n                                    </div>\n                                </b-col>\n                                <b-col xxl=\"2\" md=\"4\" cols=\"6\">\n                                    <div class=\"avatar-sm mt-3\">\n                                        <div class=\"avatar-title rounded bg-soft-success text-success fs-14\">SM</div>\n                                    </div>\n                                </b-col>\n                                <b-col xxl=\"2\" md=\"4\" cols=\"6\">\n                                    <div class=\"avatar-md mt-3\">\n                                        <div class=\"avatar-title rounded bg-soft-info text-info fs-16\">MD</div>\n                                    </div>\n                                </b-col>\n                                <b-col xxl=\"2\" md=\"4\" cols=\"6\">\n                                    <div class=\"avatar-lg mt-3\">\n                                        <div class=\"avatar-title rounded bg-soft-warning text-warning fs-20\">LG</div>\n                                    </div>\n                                </b-col>\n                                <b-col xxl=\"2\" md=\"4\" cols=\"6\">\n                                    <div class=\"avatar-xl mt-3\">\n                                        <div class=\"avatar-title rounded bg-soft-danger text-danger fs-22\">XL</div>\n                                    </div>\n                                </b-col>\n                            </b-row>\n                        </div>\n\n                        <div class=\"d-none code-view\">\n                            <pre class=\"language-markup\" style=\"height: 200px;\">\n<code>&lt;!-- Avatar With Content --&gt;\n&lt;div class=&quot;avatar-xxs&quot;&gt;\n&lt;div class=&quot;avatar-title rounded bg-soft-primary text-primary&quot;&gt;\nXXS\n&lt;/div&gt;\n&lt;/div&gt;</code>\n\n<code>&lt;div class=&quot;avatar-xs&quot;&gt;\n&lt;div class=&quot;avatar-title rounded bg-soft-secondary text-secondary&quot;&gt;\nXS\n&lt;/div&gt;\n&lt;/div&gt;</code>\n\n<code>&lt;div class=&quot;avatar-sm&quot;&gt;\n&lt;div class=&quot;avatar-title rounded bg-soft-success text-success&quot;&gt;\nSm\n&lt;/div&gt;\n&lt;/div&gt;</code>\n\n<code>&lt;div class=&quot;avatar-md&quot;&gt;\n&lt;div class=&quot;avatar-title rounded bg-soft-info text-info&quot;&gt;\nMd\n&lt;/div&gt;\n&lt;/div&gt;</code>\n\n<code>&lt;div class=&quot;avatar-lg&quot;&gt;\n&lt;div class=&quot;avatar-title rounded bg-soft-warning text-warning&quot;&gt;\nLg\n&lt;/div&gt;\n&lt;/div&gt;</code>\n\n<code>&lt;div class=&quot;avatar-xl&quot;&gt;\n&lt;div class=&quot;avatar-title rounded bg-soft-danger text-danger&quot;&gt;\nXl\n&lt;/div&gt;\n&lt;/div&gt;</code></pre>\n                        </div>\n                    </b-card-body>\n                </b-card>\n            </b-col>\n        </b-row>\n\n        <b-row>\n            <b-col lg=\"12\">\n                <b-card no-body>\n                    <b-card-header class=\"align-items-center d-flex\">\n                        <b-card-title class=\"mb-0 flex-grow-1\">Avatar Group</b-card-title>\n                        <div class=\"flex-shrink-0\">\n                            <div class=\"form-check form-switch form-switch-right form-switch-md\">\n                                <label for=\"avatar-group\" class=\"form-label text-muted\">Show Code</label>\n                                <input class=\"form-check-input code-switcher\" type=\"checkbox\" id=\"avatar-group\" />\n                            </div>\n                        </div>\n                    </b-card-header>\n\n                    <b-card-body>\n                        <div class=\"live-preview\">\n                            <b-row>\n                                <b-col lg=\"6\">\n                                    <div class=\"mt-lg-0 mt-3\">\n                                        <p class=\"text-muted\">\n                                            Use\n                                            <code>avatar-group</code> class to show avatar images with the group.\n                                        </p>\n                                        <div class=\"avatar-group\">\n                                            <div class=\"avatar-group-item\">\n                                                <img src=\"@/assets/images/users/avatar-4.jpg\" alt\n                                                    class=\"rounded-circle avatar-sm\" />\n                                            </div>\n                                            <div class=\"avatar-group-item\">\n                                                <img src=\"@/assets/images/users/avatar-5.jpg\" alt\n                                                    class=\"rounded-circle avatar-sm\" />\n                                            </div>\n                                            <div class=\"avatar-group-item\">\n                                                <div class=\"avatar-sm\">\n                                                    <div class=\"avatar-title rounded-circle bg-light text-primary\">A\n                                                    </div>\n                                                </div>\n                                            </div>\n                                            <div class=\"avatar-group-item\">\n                                                <img src=\"@/assets/images/users/avatar-2.jpg\" alt\n                                                    class=\"rounded-circle avatar-sm\" />\n                                            </div>\n                                        </div>\n                                    </div>\n                                </b-col>\n\n                                <b-col lg=\"6\">\n                                    <div class=\"mt-lg-0 mt-3\">\n                                        <p class=\"text-muted\">\n                                            Use\n                                            <code>avatar-group</code> class with\n                                            <code>data-bs-toggle=\"tooltip\"</code> to show avatar group images with\n                                            tooltip.\n                                        </p>\n                                        <div class=\"avatar-group\">\n                                            <b-link href=\"javascript: void(0);\" class=\"avatar-group-item\"\n                                                data-bs-toggle=\"tooltip\" data-bs-placement=\"top\" title=\"Christi\">\n                                                <img src=\"@/assets/images/users/avatar-4.jpg\" alt\n                                                    class=\"