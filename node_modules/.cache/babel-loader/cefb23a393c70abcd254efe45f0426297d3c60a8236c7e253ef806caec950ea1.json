{"ast":null,"code":"import Layout from \"../../layouts/main.vue\";\nimport PageHeader from \"@/components/page-header\";\nimport 'prismjs';\nimport 'prismjs/themes/prism.css';\nexport default {\n  data() {\n    return {\n      title: \"Ribbons\",\n      items: [{\n        text: \"Base UI\",\n        href: \"/\"\n      }, {\n        text: \"Ribbons\",\n        active: true\n      }]\n    };\n  },\n  name: \"Ribbons\",\n  components: {\n    Layout,\n    PageHeader\n  },\n  mounted() {\n    const checkbox = document.getElementsByClassName(\"code-switcher\");\n    Array.from(checkbox).forEach(check => {\n      check.addEventListener('change', () => {\n        const card = check.closest('.card');\n        const preview = card.querySelector(\".live-preview\");\n        const code = card.querySelector(\".code-view\");\n        if (check.checked) {\n          // do this\n          preview.classList.add(\"d-none\");\n          code.classList.remove(\"d-none\");\n        } else {\n          // do that\n          preview.classList.remove(\"d-none\");\n          code.classList.add(\"d-none\");\n        }\n      });\n    });\n  }\n};","map":{"version":3,"names":["Layout","PageHeader","data","title","items","text","href","active","name","components","mounted","checkbox","document","getElementsByClassName","Array","from","forEach","check","addEventListener","card","closest","preview","querySelector","code","checked","classList","add","remove"],"sources":["C:\\Users\\UMIDJON\\Desktop\\default2\\src\\views\\ui\\ribbons.vue"],"sourcesContent":["<script>\nimport Layout from \"../../layouts/main.vue\";\nimport PageHeader from \"@/components/page-header\";\n\nimport 'prismjs';\nimport 'prismjs/themes/prism.css';\n\nexport default {\n    data() {\n        return {\n            title: \"Ribbons\",\n            items: [\n                {\n                    text: \"Base UI\",\n                    href: \"/\",\n                },\n                {\n                    text: \"Ribbons\",\n                    active: true,\n                },\n            ],\n        };\n    },\n    name: \"Ribbons\",\n    components: {\n        Layout,\n        PageHeader,\n    },\n    mounted() {\n        const checkbox = document.getElementsByClassName(\"code-switcher\");\n        Array.from(checkbox).forEach((check) => {\n            check.addEventListener('change', () => {\n                const card = check.closest('.card');\n                const preview = card.querySelector(\".live-preview\");\n                const code = card.querySelector(\".code-view\");\n                if (check.checked) {\n                    // do this\n                    preview.classList.add(\"d-none\");\n                    code.classList.remove(\"d-none\");\n                } else {\n                    // do that\n                    preview.classList.remove(\"d-none\");\n                    code.classList.add(\"d-none\");\n                }\n            });\n        });\n    }\n};\n</script>\n\n<template>\n    <Layout>\n        <PageHeader :title=\"title\" :items=\"items\" />\n        <b-row>\n            <b-col lg=\"12\">\n                <b-card no-body>\n                    <b-card-header class=\"align-items-center d-flex\">\n                        <b-card-title class=\"mb-0 flex-grow-1\">Rounded Ribbon</b-card-title>\n                        <div class=\"flex-shrink-0\">\n                            <div class=\"form-check form-switch form-switch-right form-switch-md\">\n                                <label for=\"rounded-ribbon-showcode\" class=\"form-label text-muted\">Show Code</label>\n                                <input class=\"form-check-input code-switcher\" type=\"checkbox\"\n                                    id=\"rounded-ribbon-showcode\" />\n                            </div>\n                        </div>\n                    </b-card-header>\n\n                    <b-card-body>\n                        <div class=\"live-preview\">\n                      