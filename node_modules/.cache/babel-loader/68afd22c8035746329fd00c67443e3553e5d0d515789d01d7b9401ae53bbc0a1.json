{"ast":null,"code":"import Layout from \"../../layouts/main.vue\";\nimport PageHeader from \"@/components/page-header\";\nimport 'prismjs';\nimport 'prismjs/themes/prism.css';\nexport default {\n  data() {\n    return {\n      title: \"Placeholders\",\n      items: [{\n        text: \"Base UI\",\n        href: \"/\"\n      }, {\n        text: \"Placeholders\",\n        active: true\n      }]\n    };\n  },\n  name: \"Placeholders\",\n  components: {\n    Layout,\n    PageHeader\n  },\n  mounted() {\n    const checkbox = document.getElementsByClassName(\"code-switcher\");\n    Array.from(checkbox).forEach(check => {\n      check.addEventListener('change', () => {\n        const card = check.closest('.card');\n        const preview = card.querySelector(\".live-preview\");\n        const code = card.querySelector(\".code-view\");\n        if (check.checked) {\n          // do this\n          preview.classList.add(\"d-none\");\n          code.classList.remove(\"d-none\");\n        } else {\n          // do that\n          preview.classList.remove(\"d-none\");\n          code.classList.add(\"d-none\");\n        }\n      });\n    });\n  }\n};","map":{"version":3,"names":["Layout","PageHeader","data","title","items","text","href","active","name","components","mounted","checkbox","document","getElementsByClassName","Array","from","forEach","check","addEventListener","card","closest","preview","querySelector","code","checked","classList","add","remove"],"sources":["C:\\Users\\UMIDJON\\Desktop\\default2\\src\\views\\ui\\placeholders.vue"],"sourcesContent":["<script>\nimport Layout from \"../../layouts/main.vue\";\nimport PageHeader from \"@/components/page-header\";\n\nimport 'prismjs';\nimport 'prismjs/themes/prism.css';\n\nexport default {\n    data() {\n        return {\n            title: \"Placeholders\",\n            items: [\n                {\n                    text: \"Base UI\",\n                    href: \"/\",\n                },\n                {\n                    text: \"Placeholders\",\n                    active: true,\n                },\n            ],\n        };\n    },\n    name: \"Placeholders\",\n    components: {\n        Layout,\n        PageHeader,\n    },\n    mounted() {\n        const checkbox = document.getElementsByClassName(\"code-switcher\");\n        Array.from(checkbox).forEach((check) => {\n            check.addEventListener('change', () => {\n                const card = check.closest('.card');\n                const preview = card.querySelector(\".live-preview\");\n                const code = card.querySelector(\".code-view\");\n                if (check.checked) {\n                    // do this\n                    preview.classList.add(\"d-none\");\n                    code.classList.remove(\"d-none\");\n                } else {\n                    // do that\n                    preview.classList.remove(\"d-none\");\n                    code.classList.add(\"d-none\");\n                }\n            });\n        });\n    }\n};\n</script>\n\n<template>\n    <Layout>\n        <PageHeader :title=\"title\" :items=\"items\" />\n        <b-row>\n            <b-col lg=\"12\">\n                <b-card no-body>\n                    <b-card-header class=\"align-items-center d-flex\">\n                        <b-card-title class=\"mb-0 flex-grow-1\">Default Placeholder</b-card-title>\n                        <div class=\"flex-shrink-0\">\n                            <div class=\"form-check form-switch form-switch-right form-switch-md\">\n                                <label for=\"baseexample-showcode\" class=\"form-label text-muted\">Show Code</label>\n                                <input class=\"form-check-input code-switcher\" type=\"checkbox\"\n                                    id=\"baseexample-showcode\" />\n                            </div>\n                        </div>\n                    </b-card-header>\n\n                    <b-card-body>\n                        <p class=\"text-muted\">In the example below, we take a typical card component and recreate it\n                            with placeholders applied to create a “loading card”.</p>\n\n                        <div class=\"live-preview\">\n                            <b-row class=\"justify-content-center\">\n                                <b-col xl=\"7\">\n                                    <b-row class=\"justify-content-between\">\n                                        <b-col lg=\"5\" sm=\"6\">\n                                            <b-card no-body>\n                                                <img src=\"@/assets/images/small/img-1.jpg\" class=\"card-img-top\"\n                                                    alt=\"card img\" />\n\n                                                <b-card-body>\n                                                    <h5 class=\"card-title\">Card title</h5>\n                                                    <p class=\"card-text\">Some quick example text to build on the card\n                                                        title and make up the bulk of the card's content.</p>\n                                                    <b-link href=\"#\" class=\"btn btn-primary\">Go somewhere</b-link>\n                                                </b-card-body>\n                                            </b-card>\n                                        </b-col>\n                                        <b-col lg=\"5\" sm=\"6\">\n                                            <b-card no-body aria-hidden=\"true\">\n                                                <img src=\"@/assets/images/small/img-2.jpg\" class=\"card-img-top\"\n                                                    alt=\"card dummy img\" />\n                                                <b-card-body>\n                                                    <h5 class=\"card-title placeholder-glow\">\n                                                        <span class=\"placeholder col-6\"></span>\n                                                    </h5>\n                                                    <p class=\"card-text placeholder-glow\">\n                                                        <span class=\"placeholder col-7\"></span>\n                                                        <span class=\"placeholder col-4\"></span>\n                                                        <span class=\"placeholder col-4\"></span>\n                                                        <span class=\"placeholder col-6\"></span>\n                                                    </p>\n                                                    <b-link href=\"#\" tabindex=\"-1\"\n                                                        class=\"btn btn-primary disabled placeholder col-6\"></b-link>\n                                                </b-card-body>\n                                            </b-card>\n                                        </b-col>\n                                    </b-row>\n                                </b-col>\n                            </b-row>\n                        </div>\n\n                        <div class=\"d-none code-view\">\n                            <pre class=\"language-markup\" style=\"height: 275px\">\n<code>&lt;!-- Base Examples --&gt;\n&lt;b-card&gt;\n    &lt;img src=&quot;...&quot; class=&quot;card-img-top&quot; alt=&quot;card img&quot;&gt;\n    &lt;b-card no-body&gt;\n        &lt;h5 class=&quot;card-title&quot;&gt;Card title&lt;/h5&gt;\n        &lt;p class=&quot;card-text&quot;&gt;Some quick example text to build on the card title and make up the bulk of the card's content.&lt;/p&gt;\n        &lt;a href=&quot;#&quot; class=&quot;btn btn-primary&quot;&gt;Go somewhere&lt;/a&gt;\n    &lt;/b-card&gt;\n&lt;/b-card&gt;</code>\n\n<code>&lt;b-card aria-hidden=&quot;true&quot;&gt;\n    &lt;img src=&quot;assets/images/small/img-2.jpg&quot; class=&quot;card-img-top&quot; alt=&quot;card dummy img&quot;&gt;\n    &lt;b-card no-body&gt;\n      &lt;b-card title=&quot; &quot; class=&quot;placeholder-glow&quot;&gt;\n        &lt;span class=&quot;placeholder col-6&quot;&gt;&lt;/span&gt;\n      &lt;/b-card&gt;\n      &lt;b-card-text class=&quot;placeholder-glow&quot;&gt;\n        &lt;span class=&quot;placeholder col-7&quot;&gt;&lt;/span&gt;\n        &lt;span class=&quot;placeholder col-4&quot;&gt;&lt;/span&gt;\n        &lt;span class=&quot;placeholder col-4&quot;&gt;&lt;/span&gt;\n        &lt;span class=&quot;placeholder col-6&quot;&gt;&lt;/span&gt;\n      &lt;/b-card-text&gt;\n      &lt;b-link href=&quot;#&quot; tabindex=&quot;-1&quot; class=&quot;btn btn-primary disabled placeholder col-6&quot;&gt;&lt;/b-link&gt;\n    &lt;/b-card&gt;\n&lt;/b-card&gt;</code></pre>\n                        </div>\n                    </b-card-body>\n                </b-card>\n            </b-col>\n        </b-row>\n\n        <b-row>\n            <b-col xxl=\"6\">\n                <b-card no-body>\n                    <b-card-header class=\"align-items-center d-flex\">\n                        <b-card-title class=\"mb-0 flex-grow-1\">Width</b-card-title>\n                        <div class=\"flex-shrink-0\">\n                            <div class=\"form-check form-switch form-switch-right form-switch-md\">\n                                <label for=\"width-showcode\" class=\"form-label text-muted\">Show Code</label>\n                                <input class=\"form-check-input code-switcher\" type=\"checkbox\" id=\"width-showcode\" />\n                            </div>\n                        </div>\n                    </b-card-header>\n                    <b-card-body>\n                        <p class=\"text-muted\">\n                            Use\n                            <code>w-25,w-50,w-75</code> or\n                            <code>w-100</code> class to placeholder class to set different widths to the placeholder.\n                        </p>\n                        <div class=\"live-preview\">\n                            <span class=\"placeholder w-50\"></span>\n                            <span class=\"placeholder w-75\"></span>\n                            <span class=\"placeholder w-25\"></span>\n                            <span class=\"placeholder w-100\"></span>\n                        </div>\n                        <div class=\"d-none code-view\">\n                            <pre class=\"language-markup\">\n<code>&lt;!-- Width Sizing--&gt;\n&lt;div class=&quot;live-preview&quot;&gt;\n    &lt;span class=&quot;placeholder col-6&quot;&gt;&lt;/span&gt;\n    &lt;span class=&quot;placeholder w-75&quot;&gt;&lt;/span&gt;\n    &lt;span class=&quot;placeholder&quot; style=&quot;width: 25%;&quot;&gt;&lt;/span&gt;\n&lt;/div&gt;</code></pre>\n                        </div>\n                    </b-card-body>\n                </b-card>\n            </b-col>\n            <b-col xxl=\"6\">\n                <b-card no-body>\n                    <b-card-header class=\"align-items-center d-flex\">\n                        <b-card-title class=\"mb-0 flex-grow-1\">Sizing</b-card-title>\n                        <div class=\"flex-shrink-0\">\n                            <div class=\"form-check form-switch form-switch-right form-switch-md\">\n                                <label for=\"sizing-showcode\" class=\"form-label text-muted\">Show Code</label>\n                                <input class=\"form-check-input code-switcher\" type=\"checkbox\" id=\"sizing-showcode\" />\n                            </div>\n                        </div>\n                    </b-card-header>\n                    <b-card-body>\n                        <p class=\"text-muted\">\n                            Use\n                            <code>placeholder-lg</code>,\n                            <code>placeholder-sm</code>, or\n                            <code>placeholder-xs</code> class to placeholder class to set different size placeholder.\n                        </p>\n                        <div class=\"live-preview\">\n                            <b-row class=\"gap-0\">\n                                <b-col cols=\"12\">\n                                    <span class=\"placeholder placeholder-lg w-100\"></span>\n                                </b-col>\n                                <b-col cols=\"12\">\n                                    <span class=\"placeholder w-100\"></span>\n                                </b-col>\n                                <b-col cols=\"12\">\n                                    <span class=\"placeholder placeholder-sm w-100\"></span>\n                                </b-col>\n                                <b-col cols=\"12\">\n                                    <span class=\"placeholder placeholder-xs w-100\"></span>\n                                </b-col>\n                            </b-row>\n                        </div>\n                        <div class=\"d-none code-view\">\n                            <pre class=\"language-markup\">\n<code>&lt;!-- Sizing --&gt;\n&lt;span class=&quot;placeholder col-12 placeholder-lg&quot;&gt;&lt;/span&gt;</code>\n\n<code>&lt;span class=&quot;placeholder col-12&quot;&gt;&lt;/span&gt;</code>\n\n<code>&lt;span class=&quot;placeholder col-12 placeholder-sm&quot;&gt;&lt;/span&gt;</code>\n\n<code>&lt;span class=&quot;placeholder col-12 placeholder-xs&quot;&gt;&lt;/span&gt;</code></pre>\n                        </div>\n                    </b-card-body>\n                </b-card>\n            </b-col>\n        </b-row>\n\n        <b-row>\n            <b-col lg=\"12\">\n                <b-card no-body>\n                    <b-card-header class=\"align-items-center d-flex\">\n                        <b-card-title class=\"mb-0 flex-grow-1\">Color</b-card-title>\n                        <div class=\"flex-shrink-0\">\n                            <div class=\"form-check form-switch form-switch-right form-switch-md\">\n                                <label for=\"color-showcode\" class=\"form-label text-muted\">Show Code</label>\n                                <input class=\"form-check-input code-switcher\" type=\"checkbox\" id=\"color-showcode\" />\n                            </div>\n                        </div>\n                    </b-card-header>\n                    <b-card-body>\n                        <p class=\"text-muted\">\n                            Use\n                            <code>bg-</code> class with the below-mentioned color variation to set a custom color.\n                        </p>\n                        <div class=\"live-preview\">\n                            <b-row class=\"g-2\">\n                                <b-col cols=\"12\">\n                                    <span class=\"placeholder w-100\"></span>\n                                </b-col>\n                                <b-col cols=\"12\">\n                                    <span class=\"placeholder bg-primary w-100\"></span>\n                                </b-col>\n                                <b-col cols=\"12\">\n                                    <span class=\"placeholder bg-secondary w-100\"></span>\n                                </b-col>\n                                <b-col cols=\"12\">\n                                    <span class=\"placeholder bg-success w-100\"></span>\n                                </b-col>\n                                <b-col cols=\"12\">\n                                    <span class=\"placeholder bg-danger w-100\"></span>\n                                </b-col>\n                                <b-col cols=\"12\">\n                                    <span class=\"placeholder bg-warning w-100\"></span>\n                                </b-col>\n                                <b-col cols=\"12\">\n                                    <span class=\"placeholder bg-info w-100\"></span>\n                                </b-col>\n                                <b-col cols=\"12\">\n                                    <span class=\"placeholder bg-light w-100\"></span>\n                                </b-col>\n                                <b-col cols=\"12\">\n                                    <span class=\"placeholder bg-dark w-100\"></span>\n                                </b-col>\n                            </b-row>\n                        </div>\n                        <div class=\"d-none code-view\">\n                            <pre class=\"language-markup\" style=\"height: 275px\">\n<code>&lt;!-- Color --&gt;\n&lt;span class=&quot;placeholder col-12 mb-3&quot;&gt;&lt;/span&gt;</code>\n\n<code>&lt;span class=&quot;placeholder col-12 mb-3 bg-primary&quot;&gt;&lt;/span&gt;</code>\n\n<code>&lt;span class=&quot;placeholder col-12 mb-3 bg-secondary&quot;&gt;&lt;/span&gt;</code>\n\n<code>&lt;span class=&quot;placeholder col-12 mb-3 bg-success&quot;&gt;&lt;/span&gt;</code>\n\n<code>&lt;span class=&quot;placeholder col-12 mb-3 bg-danger&quot;&gt;&lt;/span&gt;</code>\n\n<code>&lt;span class=&quot;placeholder col-12 mb-3 bg-warning&quot;&gt;&lt;/span&gt;</code>\n\n<code>&lt;span class=&quot;placeholder col-12 mb-3 bg-info&quot;&gt;&lt;/span&gt;</code>\n\n<code>&lt;span class=&quot;placeholder col-12 mb-3 bg-light&quot;&gt;&lt;/span&gt;</code>\n\n<code>&lt;span class=&quot;placeholder col-12 mb-3 bg-dark&quot;&gt;&lt;/span&gt;</code></pre>\n                        </div>\n                    </b-card-body>\n                </b-card>\n            </b-col>\n        </b-row>\n    </Layout>\n</template>"],"mappings":"AACA,OAAOA,MAAK,MAAO,wBAAwB;AAC3C,OAAOC,UAAS,MAAO,0BAA0B;AAEjD,OAAO,SAAS;AAChB,OAAO,0BAA0B;AAEjC,eAAe;EACXC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,KAAK,EAAE,cAAc;MACrBC,KAAK,EAAE,CACH;QACIC,IAAI,EAAE,SAAS;QACfC,IAAI,EAAE;MACV,CAAC,EACD;QACID,IAAI,EAAE,cAAc;QACpBE,MAAM,EAAE;MACZ,CAAC;IAET,CAAC;EACL,CAAC;EACDC,IAAI,EAAE,cAAc;EACpBC,UAAU,EAAE;IACRT,MAAM;IACNC;EACJ,CAAC;EACDS,OAAOA,CAAA,EAAG;IACN,MAAMC,QAAO,GAAIC,QAAQ,CAACC,sBAAsB,CAAC,eAAe,CAAC;IACjEC,KAAK,CAACC,IAAI,CAACJ,QAAQ,CAAC,CAACK,OAAO,CAAEC,KAAK,IAAK;MACpCA,KAAK,CAACC,gBAAgB,CAAC,QAAQ,EAAE,MAAM;QACnC,MAAMC,IAAG,GAAIF,KAAK,CAACG,OAAO,CAAC,OAAO,CAAC;QACnC,MAAMC,OAAM,GAAIF,IAAI,CAACG,aAAa,CAAC,eAAe,CAAC;QACnD,MAAMC,IAAG,GAAIJ,IAAI,CAACG,aAAa,CAAC,YAAY,CAAC;QAC7C,IAAIL,KAAK,CAACO,OAAO,EAAE;UACf;UACAH,OAAO,CAACI,SAAS,CAACC,GAAG,CAAC,QAAQ,CAAC;UAC/BH,IAAI,CAACE,SAAS,CAACE,MAAM,CAAC,QAAQ,CAAC;QACnC,OAAO;UACH;UACAN,OAAO,CAACI,SAAS,CAACE,MAAM,CAAC,QAAQ,CAAC;UAClCJ,IAAI,CAACE,SAAS,CAACC,GAAG,CAAC,QAAQ,CAAC;QAChC;MACJ,CAAC,CAAC;IACN,CAAC,CAAC;EACN;AACJ,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}