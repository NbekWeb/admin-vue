{"ast":null,"code":"import { ref, onMounted } from 'vue';\nimport Layout from '../../layouts/main.vue';\nimport PageHeader from '@/components/page-header.vue';\nimport { getTariff_API } from '../../service/tariff';\n\n// Title va breadcrumbs\n\nexport default {\n  __name: 'tarif',\n  setup(__props, {\n    expose\n  }) {\n    expose();\n    const title = 'Таблицы';\n    const items = [{\n      text: 'Tables',\n      href: '/'\n    }, {\n      text: 'Basic Tables',\n      active: true\n    }];\n\n    // Reactive state\n    const tariffs = ref([]);\n    const showModal = ref(false);\n    const loading = ref(false);\n\n    // Tariflarni olish\n    const fetchTariffs = async () => {\n      loading.value = true;\n      const [error, data] = await getTariff_API();\n      if (!error) {\n        tariffs.value = data.results;\n      } else {\n        console.error('Tariff API xatosi:', error);\n      }\n      loading.value = false;\n    };\n\n    // Modalda saqlash tugmasi\n    const submitTariff = () => {\n      // tarif yuborish funksiyasi\n      showModal.value = false;\n    };\n\n    // Mounted\n    onMounted(() => {\n      fetchTariffs();\n    });\n    const __returned__ = {\n      title,\n      items,\n      tariffs,\n      showModal,\n      loading,\n      fetchTariffs,\n      submitTariff,\n      ref,\n      onMounted,\n      Layout,\n      PageHeader,\n      get getTariff_API() {\n        return getTariff_API;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMounted","Layout","PageHeader","getTariff_API","title","items","text","href","active","tariffs","showModal","loading","fetchTariffs","value","error","data","results","console","submitTariff"],"sources":["C:/Users/UMIDJON/Desktop/default2/src/views/tables/tarif.vue"],"sourcesContent":["<template>\n  <Layout>\n    <PageHeader :title=\"title\" :items=\"items\" />\n\n    <el-card shadow=\"never\">\n      <div class=\"card-header\">\n        <span class=\"card-title\">Тарифы</span>\n        <el-button type=\"primary\" icon=\"el-icon-plus\" @click=\"showModal.value = true\">\n          Создать тариф\n        </el-button>\n      </div>\n\n      <div class=\"live-preview\" style=\"margin-top: 16px\">\n        <el-table\n          :data=\"tariffs\"\n          border\n          stripe\n          style=\"width: 100%\"\n          v-loading=\"loading.value\"\n          empty-text=\"Тарифы не найдены\"\n        >\n          <el-table-column label=\"ID\" width=\"100\">\n            <template #default=\"{ row }\">\n              <a href=\"#\">#{{ row.code || row.id }}</a>\n            </template>\n          </el-table-column>\n\n          <el-table-column prop=\"name\" label=\"Название\" />\n          <el-table-column prop=\"price\" label=\"Цена, руб\" />\n          <el-table-column prop=\"period\" label=\"Период\" />\n\n          <el-table-column label=\"Статус\">\n            <template #default>\n              <el-tag type=\"success\">Действующий</el-tag>\n            </template>\n          </el-table-column>\n\n          <el-table-column width=\"100\">\n            <template #default>\n              <el-link type=\"success\" href=\"javascript:void(0);\">\n                Ред <i class=\"el-icon-arrow-right\"></i>\n              </el-link>\n            </template>\n          </el-table-column>\n        </el-table>\n      </div>\n    </el-card>\n\n    <!-- Modal -->\n    <el-dialog title=\"Создать тариф\" v-model=\"showModal.value\">\n      <div>Modal kontent bu yerda bo‘ladi</div>\n      <template #footer>\n        <el-button @click=\"showModal.value = false\">Отмена</el-button>\n        <el-button type=\"primary\" @click=\"submitTariff\">Сохранить</el-button>\n      </template>\n    </el-dialog>\n  </Layout>\n</template>\n\n<script setup>\nimport { ref, onMounted } from 'vue'\nimport Layout from '../../layouts/main.vue'\nimport PageHeader from '@/components/page-header.vue'\nimport { getTariff_API } from '../../service/tariff'\n\n// Title va breadcrumbs\nconst title = 'Таблицы'\nconst items = [\n  { text: 'Tables', href: '/' },\n  { text: 'Basic Tables', active: true }\n]\n\n// Reactive state\nconst tariffs = ref([])\nconst showModal = ref(false)\nconst loading = ref(false)\n\n// Tariflarni olish\nconst fetchTariffs = async () => {\n  loading.value = true\n  const [error, data] = await getTariff_API()\n  if (!error) {\n    tariffs.value = data.results\n  } else {\n    console.error('Tariff API xatosi:', error)\n  }\n  loading.value = false\n}\n\n// Modalda saqlash tugmasi\nconst submitTariff = () => {\n  // tarif yuborish funksiyasi\n  showModal.value = false\n}\n\n// Mounted\nonMounted(() => {\n  fetchTariffs()\n})\n</script>\n\n<style scoped>\n.card-header {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  font-weight: 600;\n  font-size: 18px;\n  margin-bottom: 10px;\n}\n.badge {\n  font-size: 13px;\n}\n</style>\n"],"mappings":"AA4DA,SAASA,GAAG,EAAEC,SAAS,QAAQ,KAAK;AACpC,OAAOC,MAAM,MAAM,wBAAwB;AAC3C,OAAOC,UAAU,MAAM,8BAA8B;AACrD,SAASC,aAAa,QAAQ,sBAAsB;;AAEpD;;;;;;;;IACA,MAAMC,KAAK,GAAG,SAAS;IACvB,MAAMC,KAAK,GAAG,CACZ;MAAEC,IAAI,EAAE,QAAQ;MAAEC,IAAI,EAAE;IAAI,CAAC,EAC7B;MAAED,IAAI,EAAE,cAAc;MAAEE,MAAM,EAAE;IAAK,CAAC,CACvC;;IAED;IACA,MAAMC,OAAO,GAAGV,GAAG,CAAC,EAAE,CAAC;IACvB,MAAMW,SAAS,GAAGX,GAAG,CAAC,KAAK,CAAC;IAC5B,MAAMY,OAAO,GAAGZ,GAAG,CAAC,KAAK,CAAC;;IAE1B;IACA,MAAMa,YAAY,GAAG,MAAAA,CAAA,KAAY;MAC/BD,OAAO,CAACE,KAAK,GAAG,IAAI;MACpB,MAAM,CAACC,KAAK,EAAEC,IAAI,CAAC,GAAG,MAAMZ,aAAa,CAAC,CAAC;MAC3C,IAAI,CAACW,KAAK,EAAE;QACVL,OAAO,CAACI,KAAK,GAAGE,IAAI,CAACC,OAAO;MAC9B,CAAC,MAAM;QACLC,OAAO,CAACH,KAAK,CAAC,oBAAoB,EAAEA,KAAK,CAAC;MAC5C;MACAH,OAAO,CAACE,KAAK,GAAG,KAAK;IACvB,CAAC;;IAED;IACA,MAAMK,YAAY,GAAGA,CAAA,KAAM;MACzB;MACAR,SAAS,CAACG,KAAK,GAAG,KAAK;IACzB,CAAC;;IAED;IACAb,SAAS,CAAC,MAAM;MACdY,YAAY,CAAC,CAAC;IAChB,CAAC,CAAC","ignoreList":[]},"metadata":{},"sourceTyp