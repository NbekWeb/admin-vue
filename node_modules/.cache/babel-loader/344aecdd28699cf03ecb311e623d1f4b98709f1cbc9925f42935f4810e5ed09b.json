{"ast":null,"code":"import Layout from \"../../layouts/main.vue\";\nimport PageHeader from \"@/components/page-header\";\nimport appConfig from \"../../../app.config\";\nimport \"prismjs\";\nimport \"prismjs/themes/prism.css\";\nexport default {\n  page: {\n    title: \"Basic\",\n    meta: [{\n      name: \"description\",\n      content: appConfig.description\n    }]\n  },\n  data() {\n    return {\n      title: \"Таблицы\",\n      items: [{\n        text: \"Tables\",\n        href: \"/\"\n      }, {\n        text: \"Basic Tables\",\n        active: true\n      }]\n    };\n  },\n  components: {\n    Layout,\n    PageHeader\n  },\n  mounted() {\n    const checkbox = document.getElementsByClassName(\"code-switcher\");\n    Array.from(checkbox).forEach(check => {\n      check.addEventListener(\"change\", () => {\n        const card = check.closest(\".card\");\n        const preview = card.querySelector(\".live-preview\");\n        const code = card.querySelector(\".code-view\");\n        if (check.checked) {\n          // do this\n          preview.classList.add(\"d-none\");\n          code.classList.remove(\"d-none\");\n        } else {\n          // do that\n          preview.classList.remove(\"d-none\");\n          code.classList.add(\"d-none\");\n        }\n      });\n    });\n  }\n};","map":{"version":3,"names":["Layout","PageHeader","appConfig","page","title","meta","name","content","description","data","items","text","href","active","components","mounted","checkbox","document","getElementsByClassName","Array","from","forEach","check","addEventListener","card","closest","preview","querySelector","code","checked","classList","add","remove"],"sources":["C:\\Users\\UMIDJON\\Desktop\\default2\\src\\views\\tables\\tarif.vue"],"sourcesContent":["<script>\nimport Layout from \"../../layouts/main.vue\";\nimport PageHeader from \"@/components/page-header\";\nimport appConfig from \"../../../app.config\";\n\nimport \"prismjs\";\nimport \"prismjs/themes/prism.css\";\n\n\nexport default {\n    page: {\n        title: \"Basic\",\n        meta: [{ name: \"description\", content: appConfig.description }],\n    },\n    data() {\n        return {\n            title: \"Таблицы\",\n            items: [\n                {\n                    text: \"Tables\",\n                    href: \"/\",\n                },\n                {\n                    text: \"Basic Tables\",\n                    active: true,\n                },\n            ],\n        };\n    },\n    components: {\n        Layout,\n        PageHeader,\n    },\n    mounted() {\n        const checkbox = document.getElementsByClassName(\"code-switcher\");\n        Array.from(checkbox).forEach((check) => {\n            check.addEventListener(\"change\", () => {\n                const card = check.closest(\".card\");\n                const preview = card.querySelector(\".live-preview\");\n                const code = card.querySelector(\".code-view\");\n                if (check.checked) {\n                    // do this\n                    preview.classList.add(\"d-none\");\n                    code.classList.remove(\"d-none\");\n                } else {\n                    // do that\n                    preview.classList.remove(\"d-none\");\n                    code.classList.add(\"d-none\");\n                }\n            });\n        });\n    },\n};\n</script>\n\n<template>\n    <Layout>\n        <PageHeader :title=\"title\" :items=\"items\" />\n        <b-row>\n            <b-col xl=\"6\">\n                <b-card no-body>\n                    <b-card-header class=\"align-items-center d-flex\">\n                        <b-card-title class=\"mb-0 flex-grow-1\">Терифы</b-card-title>\n                        <div class=\"flex-shrink-0\">\n                            <div class=\"form-check form-switch form-switch-right form-switch-md\">\n                                <b-button variant=\"primary\" class=\"w-100\" id=\"btn-new-event\" @click=\"showModal = true\">\n        