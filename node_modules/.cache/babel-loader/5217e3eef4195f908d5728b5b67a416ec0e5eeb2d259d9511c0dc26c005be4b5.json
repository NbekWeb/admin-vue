{"ast":null,"code":"import Layout from \"../../../../layouts/main.vue\";\nimport PageHeader from \"@/components/page-header\";\nimport appConfig from \"../../../../../app.config\";\nimport { basicLineChart, zoomableChart, lineDatalabelChart, lineAnnotation, syncingAreaChart, brushLineChart, brushAreaChart, steplineChart, gradientLineChart, realtimeLineChart, dashedLineChart, missingDataChart } from \"./data\";\n\n/**\n * Line chart component\n */\nexport default {\n  page: {\n    title: \"Line Charts\",\n    meta: [{\n      name: \"description\",\n      content: appConfig.description\n    }]\n  },\n  data() {\n    return {\n      basicLineChart: basicLineChart,\n      zoomableChart: zoomableChart,\n      lineDatalabelChart: lineDatalabelChart,\n      lineAnnotation: lineAnnotation,\n      syncingAreaChart: syncingAreaChart,\n      brushLineChart: brushLineChart,\n      brushAreaChart: brushAreaChart,\n      steplineChart: steplineChart,\n      gradientLineChart: gradientLineChart,\n      realtimeLineChart: realtimeLineChart,\n      dashedLineChart: dashedLineChart,\n      missingDataChart: missingDataChart,\n      title: \"Line Charts\",\n      items: [{\n        text: \"Apexcharts\",\n        href: \"/\"\n      }, {\n        text: \"Line Charts\",\n        active: true\n      }]\n    };\n  },\n  components: {\n    Layout,\n    PageHeader\n  }\n};","map":{"version":3,"names":["Layout","PageHeader","appConfig","basicLineChart","zoomableChart","lineDatalabelChart","lineAnnotation","syncingAreaChart","brushLineChart","brushAreaChart","steplineChart","gradientLineChart","realtimeLineChart","dashedLineChart","missingDataChart","page","title","meta","name","content","description","data","items","text","href","active","components"],"sources":["C:\\Users\\UMIDJON\\Desktop\\default2\\src\\views\\charts\\apex\\line\\index.vue"],"sourcesContent":["<script>\nimport Layout from \"../../../../layouts/main.vue\";\nimport PageHeader from \"@/components/page-header\";\nimport appConfig from \"../../../../../app.config\";\n\nimport {\n  basicLineChart,\n  zoomableChart,\n  lineDatalabelChart,\n  lineAnnotation,\n  syncingAreaChart,\n  brushLineChart,\n  brushAreaChart,\n  steplineChart,\n  gradientLineChart,\n  realtimeLineChart,\n  dashedLineChart,\n  missingDataChart,\n} from \"./data\";\n\n/**\n * Line chart component\n */\nexport default {\n  page: {\n    title: \"Line Charts\",\n    meta: [\n      {\n        name: \"description\",\n        content: appConfig.description,\n      },\n    ],\n  },\n  data() {\n    return {\n      basicLineChart: basicLineChart,\n      zoomableChart: zoomableChart,\n      lineDatalabelChart: lineDatalabelChart,\n      lineAnnotation: lineAnnotation,\n      syncingAreaChart: syncingAreaChart,\n      brushLineChart: brushLineChart,\n      brushAreaChart: brushAreaChart,\n      steplineChart: steplineChart,\n      gradientLineChart: gradientLineChart,\n      realtimeLineChart: realtimeLineChart,\n      dashedLineChart: dashedLineChart,\n      missingDataChart: missingDataChart,\n      title: \"Line Charts\",\n      items: [\n        {\n          text: \"Apexcharts\",\n          href: \"/\",\n        },\n        {\n          text: \"Line Charts\",\n          active: true,\n        },\n      ],\n\n    };\n  },\n  components: {\n    Layout,\n    PageHeader,\n  },\n};\n</script>\n\n<template>\n  <Layout>\n    <PageHeader :title=\"title\" :items=\"items\" />\n    <b-row>\n      <b-col lg=\"6\">\n        <b-card no-body>\n          <b-card-header>\n            <b-card-title class=\"mb-0\">Basic Line Chart</b-card-title>\n          </b-card-header>\n          <b-card-body>\n            <apexchart class=\"apex-charts\" height=\"350\" dir=\"ltr\" :series=\"basicLineChart.series\"\n              :options=\"basicLineChart.chartOptions\"></apexchart>\n          </b-card-body>\n        </b-card>\n      </b-col>\n\n      <b-col lg=\"6\">\n        <b-card no-body>\n          <b-card-header>\n            <b-card-title class=\"mb-0\">Zoomable Timeseries</b-card-title>\n          </b-card-header>\n          <b-card-body>\n            <apexchart class=\"apex-charts\" height=\"350\" dir=\"ltr\" :series=\"zoomableChart.series\"\n              :options=\"zoomableChart.chartOptions\"></apexchart>\n          </b-card-body>\n        </b-card>\n      </b-col>\n    </b-row>\n\n    <b-row>\n      <b-col lg=\"6\">\n        <b-card no-body>\n          <b-card-header>\n            <b-card-title class=\"mb-0\">Line with Data Labels</b-card-title>\n          </b-card-header>\n          <b-card-body>\n            <apexchart class=\"apex-charts\" height=\"350\" dir=\"ltr\" :series=\"lineDatalabelChart.series\"\n              :options=\"lineDatalabelChart.chartOptions\"></apexchart>\n          </b-card-body>\n        </b-card>\n      </b-col>\n\n      <b-col lg=\"6\">\n        <b-card no-body>\n          <b-card-header>\n            <b-card-title class=\"mb-0\">Dashed Line</b-card-title>\n          </b-card-header>\n          <b-card-body>\n            <apexchart class=\"apex-charts\" height=\"350\" dir=\"ltr\" :series=\"dashedLineChart.series\"\n              :options=\"dashedLineChart.chartOptions\"></apexchart>\n          </b-card-body>\n        </b-card>\n      </b-col>\n    </b-row>\n\n    <b-row>\n      <b-col lg=\"6\">\n        <b-card no-body>\n          <b-card-header>\n            <b-card-title class=\"mb-0\">Line with Annotations</b-card-title>\n          </b-card-header>\n          <b-card-body>\n            <apexchart class=\"apex-charts\" height=\"350\" dir=\"ltr\" :series=\"lineAnnotation.series\"\n              :options=\"lineAnnotation.chartOptions\"></apexchart>\n          </b-card-body>\n        </b-card>\n      </b-col>\n      <b-col lg=\"6\">\n        <b-card no-body>\n          <b-card-header>\n            <b-card-title class=\"mb-0\">Brush Charts</b-card-title>\n          </b-card-header>\n          <b-card-body>\n            <div>\n              <apexchart class=\"apex-charts\" height=