{"ast":null,"code":"import SwiperCore, { Autoplay, Thumbs, Pagination, Navigation } from \"swiper\";\nimport { Swiper, SwiperSlide } from \"swiper/vue\";\nimport \"swiper/swiper-bundle.css\";\nSwiperCore.use([Autoplay, Thumbs, Pagination, Navigation]);\nimport Layout from \"../../layouts/main.vue\";\nimport PageHeader from \"@/components/page-header\";\nimport appConfig from \"../../../app.config\";\nexport default {\n  page: {\n    title: \"Swiper\",\n    meta: [{\n      name: \"description\",\n      content: appConfig.description\n    }]\n  },\n  data() {\n    return {\n      title: \"Swiper Slider\",\n      items: [{\n        text: \"Advance UI\",\n        href: \"/\"\n      }, {\n        text: \"Swiper Slider\",\n        active: true\n      }]\n    };\n  },\n  components: {\n    Layout,\n    PageHeader,\n    Swiper,\n    SwiperSlide\n  }\n};","map":{"version":3,"names":["SwiperCore","Autoplay","Thumbs","Pagination","Navigation","Swiper","SwiperSlide","use","Layout","PageHeader","appConfig","page","title","meta","name","content","description","data","items","text","href","active","components"],"sources":["C:\\Users\\UMIDJON\\Desktop\\default2\\src\\views\\advance-ui\\swiper.vue"],"sourcesContent":["<script>\nimport SwiperCore, { Autoplay,Thumbs, Pagination, Navigation } from \"swiper\";\nimport { Swiper, SwiperSlide } from \"swiper/vue\";\nimport \"swiper/swiper-bundle.css\";\n\nSwiperCore.use([Autoplay,Thumbs, Pagination, Navigation]);\n\nimport Layout from \"../../layouts/main.vue\";\nimport PageHeader from \"@/components/page-header\";\nimport appConfig from \"../../../app.config\";\n\nexport default {\n  page: {\n    title: \"Swiper\",\n    meta: [{ name: \"description\", content: appConfig.description }],\n  },\n  data() {\n    return {\n      title: \"Swiper Slider\",\n      items: [\n        {\n          text: \"Advance UI\",\n          href: \"/\",\n        },\n        {\n          text: \"Swiper Slider\",\n          active: true,\n        },\n      ],\n    };\n  },\n  components: {\n    Layout,\n    PageHeader,\n    Swiper,\n    SwiperSlide,\n  },\n};\n</script>\n\n<template>\n  <Layout>\n    <PageHeader :title=\"title\" :items=\"items\" />\n\n    <b-row>\n      <b-col xl=\"4\" lg=\"6\">\n        <b-card no-body>\n          <b-card-header>\n            <b-card-title class=\"mb-0\">Default Swiper</b-card-title>\n          </b-card-header>\n\n          <b-card-body>\n            <p class=\"text-muted\">\n              Use\n              <code>default-swiper</code> class to set a default swiper.\n            </p>\n            <swiper class=\"default-swiper rounded\" :autoplay=\"{\n              delay: 2500,\n              disableOnInteraction: false,\n            }\">\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-1.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-2.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-3.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n            </swiper>\n          </b-card-body>\n        </b-card>\n      </b-col>\n      <b-col xl=\"4\" lg=\"6\">\n        <b-card no-body>\n          <b-card-header>\n            <b-card-title class=\"mb-0\">Navigation & Pagination Swiper</b-card-title>\n          </b-card-header>\n\n          <b-card-body>\n            <p class=\"text-muted\">\n              Use\n              <code>navigation-swiper</code> class to set a swiper with\n              navigation and pagination.\n            </p>\n            <swiper class=\"navigation-swiper rounded\" :loop=\"true\" :autoplay=\"{\n              delay: 2500,\n              disableOnInteraction: false,\n            }\" :navigation=\"{\n  nextEl: '.swiper-button-next',\n  prevEl: '.swiper-button-prev',\n}\" :pagination=\"{\n  clickable: true,\n  el: '.swiper-pagination',\n}\">\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-4.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-5.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-6.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <div class=\"swiper-button-next\"></div>\n              <div class=\"swiper-button-prev\"></div>\n              <div class=\"swiper-pagination\"></div>\n            </swiper>\n          </b-card-body>\n        </b-card>\n      </b-col>\n\n      <b-col xl=\"4\" lg=\"6\">\n        <b-card no-body>\n          <b-card-header>\n            <b-card-title class=\"mb-0\">Pagination Dynamic Swiper</b-card-title>\n          </b-card-header>\n\n          <b-card-body>\n            <p class=\"text-muted\">\n              Use\n              <code>pagination-dynamic-swiper</code> class to set a dynamic\n              swiper with pagination.\n            </p>\n            <swiper class=\"pagination-dynamic-swiper rounded\" :loop=\"true\" :autoplay=\"{\n              delay: 2500,\n              disableOnInteraction: false,\n            }\" :pagination=\"{\n  clickable: true,\n  el: '.swiper-pagination',\n  dynamicBullets: true,\n}\">\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-7.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-8.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-9.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <div class=\"swiper-pagination dynamic-pagination\"></div>\n            </swiper>\n          </b-card-body>\n        </b-card>\n      </b-col>\n\n      <b-col xl=\"4\" lg=\"6\">\n        <b-card no-body>\n          <b-card-header>\n            <b-card-title class=\"mb-0\">Pagination Fraction Swiper</b-card-title>\n          </b-card-header>\n          <b-card-body>\n            <p class=\"text-muted\">\n              Use\n              <code>pagination-fraction-swiper</code> class to set a\n              fraction swiper with pagination.\n            </p>\n            <swiper class=\"pagination-fraction-swiper\" :loop=\"true\" :autoplay=\"{\n              delay: 2500,\n              disableOnInteraction: false,\n            }\" :pagination=\"{\n  clickable: true,\n  el: '.swiper-pagination',\n  type: 'fraction',\n}\" :navigation=\"{\n  nextEl: '.swiper-button-next',\n  prevEl: '.swiper-button-prev',\n}\">\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-10.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-11.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-12.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <div class=\"swiper-button-next\"></div>\n              <div class=\"swiper-button-prev\"></div>\n              <div class=\"swiper-pagination\"></div>\n            </swiper>\n          </b-card-body>\n        </b-card>\n      </b-col>\n\n      <b-col xl=\"4\" lg=\"6\">\n        <b-card no-body>\n          <b-card-header>\n            <b-card-title class=\"mb-0\">Pagination Custom Swiper</b-card-title>\n          </b-card-header>\n          <b-card-body>\n            <p class=\"text-muted\">\n              Use\n              <code>pagination-custom-swiper</code> class to set a swiper\n              with custom pagination.\n            </p>\n            <swiper class=\"pagination-custom-swiper rounded\" :loop=\"true\" :autoplay=\"{\n              delay: 2500,\n              disableOnInteraction: false,\n            }\" :pagination=\"{\n  clickable: true,\n  el: '.swiper-pagination',\n  renderBullet: function (index, className) {\n    return '<span class=' + className + '>' + (index + 1) + '</span>';\n  },\n}\">\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-2.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-3.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-4.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <div class=\"swiper-pagination pagination-custom\"></div>\n            </swiper>\n          </b-card-body>\n        </b-card>\n      </b-col>\n\n      <b-col xl=\"4\" lg=\"6\">\n        <b-card no-body>\n          <b-card-header>\n            <b-card-title class=\"mb-0\">Pagination Progress Swiper</b-card-title>\n          </b-card-header>\n          <b-card-body>\n            <p class=\"text-muted\">\n              Use\n              <code>pagination-progress-swiper</code> class to set a swiper\n              with progress pagination.\n            </p>\n            <swiper class=\"pagination-progress-swiper rounded\" :loop=\"true\" :autoplay=\"{\n              delay: 2500,\n              disableOnInteraction: false,\n            }\" :pagination=\"{\n  clickable: true,\n  el: '.swiper-pagination',\n  type: 'fraction',\n}\" :navigation=\"{\n  nextEl: '.swiper-button-next',\n  prevEl: '.swiper-button-prev',\n}\">\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-5.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-6.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-7.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <div class=\"swiper-button-next\"></div>\n              <div class=\"swiper-button-prev\"></div>\n              <div class=\"swiper-pagination\"></div>\n            </swiper>\n          </b-card-body>\n        </b-card>\n      </b-col>\n\n      <b-col xl=\"4\" lg=\"6\">\n        <b-card no-body>\n          <b-card-header>\n            <b-card-title class=\"mb-0\">Scrollbar Swiper</b-card-title>\n          </b-card-header>\n          <b-card-body>\n            <p class=\"text-muted\">\n              Use\n              <code>pagination-scrollbar-swiper</code> class to set a swiper\n              with scrollbar pagination.\n            </p>\n            <swiper class=\"pagination-scrollbar-swiper rounded\" :loop=\"true\" :autoplay=\"{\n              delay: 2500,\n              disableOnInteraction: false,\n            }\" :scrollbar=\"{\n  el: '.swiper-scrollbar',\n  hide: true,\n}\" :navigation=\"{\n  nextEl: '.swiper-button-next',\n  prevEl: '.swiper-button-prev',\n}\">\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-8.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-9.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-10.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <div class=\"swiper-button-next\"></div>\n              <div class=\"swiper-button-prev\"></div>\n              <div class=\"swiper-scrollbar\"></div>\n            </swiper>\n          </b-card-body>\n        </b-card>\n      </b-col>\n\n      <b-col xl=\"4\" lg=\"6\">\n        <b-card no-body>\n          <b-card-header>\n            <b-card-title class=\"mb-0\">Vertical Swiper</b-card-title>\n          </b-card-header>\n\n          <b-card-body>\n            <p class=\"text-muted\">\n              Use\n              <code>vertical-swiper</code> class to set a vertical swiper.\n            </p>\n            <swiper class=\"vertical-swiper rounded\" :loop=\"true\" :direction=\"'vertical'\" :autoplay=\"{\n              delay: 2500,\n              disableOnInteraction: false,\n            }\" :pagination=\"{\n  el: '.swiper-pagination',\n  clickable: true,\n}\" style=\"height: 324px\">\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-11.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-12.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-1.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <div class=\"swiper-pagination\"></div>\n            </swiper>\n          </b-card-body>\n        </b-card>\n      </b-col>\n\n\n      <b-col xl=\"4\" lg=\"6\">\n        <b-card no-body>\n          <b-card-header>\n            <b-card-title class=\"mb-0\">Mousewheel Control Swiper</b-card-title>\n          </b-card-header>\n          <b-card-body>\n            <p class=\"text-muted\">\n              Use\n              <code>mousewheel-control-swiper</code> class to set a swiper\n              with mousewheel scroll.\n            </p>\n            <swiper class=\"mousewheel-control-swiper rounded\" :loop=\"true\" :direction=\"'vertical'\" :mousewheel=\"true\"\n              :autoplay=\"{\n                delay: 2500,\n                disableOnInteraction: false,\n              }\" :pagination=\"{\n  el: '.swiper-pagination',\n  clickable: true,\n}\" style=\"height: 324px\">\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-3.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-4.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-5.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <div class=\"swiper-pagination\"></div>\n            </swiper>\n          </b-card-body>\n        </b-card>\n      </b-col>\n\n\n      <b-col xl=\"4\" lg=\"6\">\n        <b-card no-body>\n          <b-card-header>\n            <b-card-title class=\"mb-0\">Effect Fade Swiper</b-card-title>\n          </b-card-header>\n          <b-card-body>\n            <p class=\"text-muted\">\n              Use\n              <code>effect-fade-swiper</code> class to set a swiper with\n              fade effect.\n            </p>\n            <swiper class=\"effect-fade-swiper rounded\" :loop=\"true\" :effect=\"'fade'\" :autoplay=\"{\n              delay: 2500,\n              disableOnInteraction: false,\n            }\" :pagination=\"{\n  el: '.swiper-pagination',\n  clickable: true,\n}\">\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-6.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-7.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-8.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <div class=\"swiper-pagination\"></div>\n            </swiper>\n          </b-card-body>\n        </b-card>\n      </b-col>\n\n      <b-col xl=\"4\" lg=\"6\">\n        <b-card no-body>\n          <b-card-header>\n            <b-card-title class=\"mb-0\">Effect Creative Swiper</b-card-title>\n          </b-card-header>\n          <b-card-body>\n            <p class=\"text-muted\">\n              Use\n              <code>effect-creative-swiper</code> class to set a swiper with\n              creative custom effect.\n            </p>\n            <swiper class=\"effect-creative-swiper rounded\" :loop=\"true\" :effect=\"'creative'\" :creativeEffect=\"{\n              prev: {\n                shadow: true,\n                translate: [0, 0, -400],\n              },\n              next: {\n                translate: ['100%', 0, 0],\n              },\n            }\" :autoplay=\"{\n  delay: 2500,\n  disableOnInteraction: false,\n}\" :pagination=\"{\n  el: '.swiper-pagination',\n  clickable: true,\n  dynamicBullets: true,\n}\">\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-9.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-10.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-11.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <div class=\"swiper-pagination\"></div>\n            </swiper>\n          </b-card-body>\n        </b-card>\n      </b-col>\n\n      <b-col xl=\"4\" lg=\"6\">\n        <b-card no-body>\n          <b-card-header>\n            <b-card-title class=\"mb-0\">Effect Flip Swiper</b-card-title>\n          </b-card-header>\n          <b-card-body>\n            <p class=\"text-muted\">\n              Use\n              <code>effect-flip-swiper</code> class to set a swiper with\n              flip effect.\n            </p>\n            <swiper class=\"effect-flip-swiper rounded\" :loop=\"true\" :effect=\"'flip'\" :grabCursor=\"true\" :autoplay=\"{\n              delay: 2500,\n              disableOnInteraction: false,\n            }\" :pagination=\"{\n  el: '.swiper-pagination',\n  clickable: true,\n}\">\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-12.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-1.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-2.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <div class=\"swiper-pagination\"></div>\n            </swiper>\n          </b-card-body>\n        </b-card>\n      </b-col>\n\n      <b-col lg=\"12\">\n        <b-card no-body>\n          <b-card-header>\n            <b-card-title class=\"mb-0\">Effect Coverflow Swiper</b-card-title>\n          </b-card-header>\n          <b-card-body>\n            <p class=\"text-muted\">\n              Use\n              <code>effect-coverflow-swiper</code> class to set a swiper\n              with coverflow effect.\n            </p>\n            <swiper class=\"effect-coverflow-swiper rounded pb-5\" :loop=\"true\" :effect=\"'coverflow'\" :grabCursor=\"true\"\n              :centeredSlides=\"true\" :slidesPerView=\"4\" :coverflowEffect=\"{\n                rotate: 50,\n                stretch: 0,\n                depth: 100,\n                modifier: 1,\n                slideShadows: true,\n              }\" :autoplay=\"{\n  delay: 2500,\n  disableOnInteraction: false,\n}\" :pagination=\"{\n  el: '.swiper-pagination',\n  clickable: true,\n  dynamicBullets: true,\n}\">\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-4.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-5.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-6.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-7.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-8.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-9.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <div class=\"swiper-pagination swiper-pagination-dark\"></div>\n            </swiper>\n          </b-card-body>\n        </b-card>\n      </b-col>\n\n\n      <b-col lg=\"12\">\n        <b-card no-body>\n          <b-card-header>\n            <b-card-title class=\"mb-0\">Responsive Breakpoints Swiper</b-card-title>\n          </b-card-header>\n\n          <b-card-body>\n            <p class=\"text-muted\">\n              Use\n              <code>responsive-swiper</code> class to set a responsive\n              swiper.\n            </p>\n            <swiper class=\"swiper responsive-swiper rounded gallery-light pb-4\" :loop=\"true\" :slidesPerView=\"1\"\n              :spaceBetween=\"10\" :pagination=\"{\n                el: '.swiper-pagination',\n                clickable: true,\n              }\" :breakpoints=\"{\n  640: {\n    slidesPerView: 2,\n    spaceBetween: 20,\n  },\n  768: {\n    slidesPerView: 3,\n    spaceBetween: 40,\n  },\n  1200: {\n    slidesPerView: 4,\n    spaceBetween: 50,\n  },\n}\">\n              <swiper-slide>\n                <b-card no-body class=\"gallery-box\">\n                  <div class=\"gallery-container\">\n                    <b-link class=\"image-popup\" href=\"assets/images/small/img-1.jpg\" title>\n                      <img class=\"gallery-img img-fluid mx-auto\" src=\"@/assets/images/small/img-1.jpg\" alt />\n                      <div class=\"gallery-overlay\">\n                        <h5 class=\"overlay-caption\">Glasses and laptop from above</h5>\n                      </div>\n                    </b-link>\n                  </div>\n                  <div class=\"box-content\">\n                    <div class=\"