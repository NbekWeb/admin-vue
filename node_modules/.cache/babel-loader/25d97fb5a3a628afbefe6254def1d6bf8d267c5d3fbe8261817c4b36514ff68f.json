{"ast":null,"code":"import Layout from \"../../layouts/main.vue\";\nimport PageHeader from \"@/components/page-header\";\nimport appConfig from \"../../../app.config\";\nimport { getTariff_API } from \"../../service/tariff\";\nexport default {\n  components: {\n    Layout,\n    PageHeader\n  },\n  data() {\n    return {\n      title: \"Таблицы\",\n      items: [{\n        text: \"Tables\",\n        href: \"/\"\n      }, {\n        text: \"Basic Tables\",\n        active: true\n      }],\n      tariffs: [],\n      loading: false,\n      showModal: false\n    };\n  },\n  mounted() {\n    this.fetchTariffs();\n  },\n  methods: {\n    fetchTariffs() {\n      this.loading = true;\n      getTariff_API().then(([error, data]) => {\n        this.loading = false;\n        if (!error) {\n          this.tariffs = data;\n        } else {\n          console.error(\"Tariff API xatosi:\", error);\n        }\n      });\n    },\n    submitTariff() {\n      // Tarif yaratish funksiyasi (keyinchalik to‘ldiriladi)\n      this.showModal = false;\n    }\n  }\n};","map":{"version":3,"names":["Layout","PageHeader","appConfig","getTariff_API","components","data","title","items","text","href","active","tariffs","loading","showModal","mounted","fetchTariffs","methods","then","error","console","submitTariff"],"sources":["C:\\Users\\UMIDJON\\Desktop\\default2\\src\\views\\tables\\tarif.vue"],"sourcesContent":["<template>\n  <Layout>\n    <PageHeader :title=\"title\" :items=\"items\" />\n\n    <el-card shadow=\"never\">\n      <div class=\"card-header\" style=\"display: flex; justify-content: space-between; align-items: center\">\n        <span style=\"font-weight: 600\">Тарифы</span>\n        <el-button type=\"primary\" icon=\"el-icon-plus\" @click=\"showModal = true\">\n          Создать тариф\n        </el-button>\n      </div>\n\n      <div class=\"live-preview\" style=\"margin-top: 16px\">\n        <el-table\n          :data=\"tariffs\"\n          stripe\n          border\n          style=\"width: 100%\"\n          v-loading=\"loading\"\n          empty-text=\"Тарифы не найдены\"\n        >\n          <el-table-column label=\"ID\" prop=\"id\" width=\"80\">\n            <template slot-scope=\"scope\">\n              <a href=\"#\" style=\"font-weight: 500\">#{{ scope.row.code || scope.row.id }}</a>\n            </template>\n          </el-table-column>\n\n          <el-table-column label=\"Название\" prop=\"name\" />\n          <el-table-column label=\"Цена, руб\" prop=\"price\" />\n          <el-table-column label=\"Период\" prop=\"period\" />\n\n          <el-table-column label=\"Статус\" width=\"120\">\n            <template slot-scope=\"scope\">\n              <el-tag type=\"success\">Действующий</el-tag>\n            </template>\n          </el-table-column>\n\n          <el-table-column label=\"\" width=\"100\">\n            <template slot-scope=\"scope\">\n              <el-link type=\"success\" href=\"javascript:void(0);\">\n                Ред <i class=\"el-icon-arrow-right\"></i>\n              </el-link>\n            </template>\n          </el-table-column>\n        </el-table>\n      </div>\n    </el-card>\n\n    <!-- Modal form uchun joy qoldirilgan -->\n    <el-dialog title=\"Создать тариф\" :visible.sync=\"showModal\">\n      <div>Modal kontenti shu yerda bo'ladi</div>\n      <span slot=\"footer\" class=\"dialog-footer\">\n        <el-button @click=\"showModal = false\">Отмена</el-button>\n        <el-button type=\"primary\" @click=\"submitTariff\">Сохранить</el-button>\n      </span>\n    </el-dialog>\n  </Layout>\n</template>\n\n<script>\nimport Layout from \"../../layouts/main.vue\";\nimport PageHeader from \"@/components/page-header\";\nimport appConfig from \"../../../app.config\";\nimport { getTariff_API } from \"../../service/tariff\";\n\nexport default {\n  components: {\n    Layout,\n    PageHeader,\n  },\n  data() {\n    return {\n      title: \"Таблицы\",\n      items: [\n        { text: \"Tables\", href: \"/\" },\n        { text: \"Basic Tables\", active: true },\n      ],\n      tariffs: [],\n      loading: false,\n      showModal: false,\n    };\n  },\n  mounted() {\n    this.fetchTariffs();\n  },\n  methods: {\n    fetchTariffs() {\n      this.loading = true;\n      getTariff_API().then(([error, data]) => {\n        this.loading = false;\n        if (!error) {\n          this.tariffs = data;\n        } else {\n          console.error(\"Tariff API xatosi:\", error);\n        }\n      });\n    },\n    submitTariff() {\n      // Tarif yaratish funksiyasi (keyinchalik to‘ldiriladi)\n      this.showModal = false;\n    },\n  },\n};\n</script>\n\n<style scoped>\n.card-header {\n  font-size: 18px;\n  padding: 10px 0;\n}\n</style>\n"],"mappings":"AA4DA,OAAOA,MAAK,MAAO,wBAAwB;AAC3C,OAAOC,UAAS,MAAO,0BAA0B;AACjD,OAAOC,SAAQ,MAAO,qBAAqB;AAC3C,SAASC,aAAY,QAAS,sBAAsB;AAEpD,eAAe;EACbC,UAAU,EAAE;IACVJ,MAAM;IACNC;EACF,CAAC;EACDI,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,SAAS;MAChBC,KAAK,EAAE,CACL;QAAEC,IAAI,EAAE,QAAQ;QAAEC,IAAI,EAAE;MAAI,CAAC,EAC7B;QAAED,IAAI,EAAE,cAAc;QAAEE,MAAM,EAAE;MAAK,CAAC,CACvC;MACDC,OAAO,EAAE,EAAE;MACXC,OAAO,EAAE,KAAK;MACdC,SAAS,EAAE;IACb,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,YAAY,CAAC,CAAC;EACrB,CAAC;EACDC,OAAO,EAAE;IACPD,YAAYA,CAAA,EAAG;MACb,IAAI,CAACH,OAAM,GAAI,IAAI;MACnBT,aAAa,CAAC,CAAC,CAACc,IAAI,CAAC,CAAC,CAACC,KAAK,EAAEb,IAAI,CAAC,KAAK;QACtC,IAAI,CAACO,OAAM,GAAI,KAAK;QACpB,IAAI,CAACM,KAAK,EAAE;UACV,IAAI,CAACP,OAAM,GAAIN,IAAI;QACrB,OAAO;UACLc,OAAO,CAACD,KAAK,CAAC,oBAAoB,EAAEA,KAAK,CAAC;QAC5C;MACF,CAAC,CAAC;IACJ,CAAC;IACDE,YAAYA,CAAA,EAAG;MACb;MACA,IAAI,CAACP,SAAQ,GAAI,KAAK;IACxB;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}