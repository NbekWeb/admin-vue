{"ast":null,"code":"import Layout from \"../../layouts/main.vue\";\nimport PageHeader from \"@/components/page-header\";\nimport 'prismjs';\nimport 'prismjs/themes/prism.css';\nexport default {\n  data() {\n    return {\n      title: \"Notifications\",\n      items: [{\n        text: \"Base UI\",\n        href: \"/\"\n      }, {\n        text: \"Notifications\",\n        active: true\n      }]\n    };\n  },\n  name: \"Notifications\",\n  components: {\n    Layout,\n    PageHeader\n  },\n  mounted() {\n    const checkbox = document.getElementsByClassName(\"code-switcher\");\n    Array.from(checkbox).forEach(check => {\n      check.addEventListener('change', () => {\n        const card = check.closest('.card');\n        const preview = card.querySelector(\".live-preview\");\n        const code = card.querySelector(\".code-view\");\n        if (check.checked) {\n          // do this\n          preview.classList.add(\"d-none\");\n          code.classList.remove(\"d-none\");\n        } else {\n          // do that\n          preview.classList.remove(\"d-none\");\n          code.classList.add(\"d-none\");\n        }\n      });\n    });\n  }\n};","map":{"version":3,"names":["Layout","PageHeader","data","title","items","text","href","active","name","components","mounted","checkbox","document","getElementsByClassName","Array","from","forEach","check","addEventListener","card","closest","preview","querySelector","code","checked","classList","add","remove"],"sources":["C:\\Users\\UMIDJON\\Desktop\\default2\\src\\views\\ui\\notifications.vue"],"sourcesContent":["<script>\nimport Layout from \"../../layouts/main.vue\";\nimport PageHeader from \"@/components/page-header\";\n\nimport 'prismjs';\nimport 'prismjs/themes/prism.css';\n\nexport default {\n    data() {\n        return {\n            title: \"Notifications\",\n            items: [\n                {\n                    text: \"Base UI\",\n                    href: \"/\",\n                },\n                {\n                    text: \"Notifications\",\n                    active: true,\n                },\n            ],\n        };\n    },\n    name: \"Notifications\",\n    components: {\n        Layout,\n        PageHeader,\n    },\n    mounted() {\n        const checkbox = document.getElementsByClassName(\"code-switcher\");\n        Array.from(checkbox).forEach((check) => {\n            check.addEventListener('change', () => {\n                const card = check.closest('.card');\n                const preview = card.querySelector(\".live-preview\");\n                const code = card.querySelector(\".code-view\");\n                if (check.checked) {\n                    // do this\n                    preview.classList.add(\"d-none\");\n                    code.classList.remove(\"d-none\");\n                } else {\n                    // do that\n                    preview.classList.remove(\"d-none\");\n                    code.classList.add(\"d-none\");\n                }\n            });\n        });\n    }\n};\n</script>\n\n<template>\n    <Layout>\n        <PageHeader :title=\"title\" :items=\"items\" />\n        <b-row>\n            <b-col lg=\"12\">\n                <b-card no-body>\n                    <b-card-header  class=\"align-items-center d-flex\">\n                        <b-card-title class=\"mb-0 flex-grow-1\">Bootstrap Toasts</b-card-title>\n                        <div class=\"flex-shrink-0\">\n                            <div class=\"form-check form-switch form-switch-right form-switch-md\">\n                                <label for=\"bootstraptoast-showcode\" class=\"form-label text-muted\">\n                                    Show\n                                    Code\n                                </label>\n                                <input class=\"form-check-input code-switcher\" type=\"checkbox\"\n                                    id=\"bootstraptoast-showcode\" />\n                            </div>\n                        </div>\n                    </b-card-header >\n\n                    <b-card-body>\n                        <div class=\"live-preview\">\n                            <b-row class=\"g-3\">\n                                <b-col xxl=\"6\">\n                                    <h5 class=\"fs-14 mb-3\">Default Toast</h5>\n                                    <p class=\"text-muted\">\n                                        Use\n                                        <code>toast</code> class to set a default toast.\n                                    </p>\n                                    <!-- Basic Toasts Example -->\n                                    <div class=\"toast fade show\" role=\"alert\" aria-live=\"assertive\"\n                                        data-bs-autohide=\"false\" aria-atomic=\"true\">\n                                        <div class=\"toast-header\">\n                                            <img src=\"@/assets/images/logo-sm.png\" class=\"rounded me-2\" alt=\"...\"\n                                                height=\"20\" />\n                                            <span class=\"fw-semibold me-auto\">Velzon</span>\n                                            <small>06 mins ago</small>\n                                            <button type=\"button\" class=\"btn-close\" data-bs-dismiss=\"toast\"\n                                                aria-label=\"Close\"></button>\n                                        </div>\n                                        <div class=\"toast-body\">Hello, world! This is a toast message.</div>\n                                    </div>\n\n                                    <div class=\"mt-4\">\n                                        <h5 class=\"fs-14\">Translucent</h5>\n                                        <p class=\"text-muted\">\n                                            Toasts are slightly translucent, too, so they\n                                            blend over whatever\n                                            they might appear over.\n                                        </p>\n                                        <!-- Toasts Translucent -->\n                                        <div class=\"p-3 bg-light\">\n                                            <div class=\"toast fade show\" role=\"alert\" aria-live=\"assertive\"\n                                                data-bs-autohide=\"false\" aria-atomic=\"true\">\n                                                <div class=\"toast-header\">\n                                                    <img src=\"@/assets/images/logo-sm.png\" class=\"rounded me-2\"\n                                                        alt=\"...\" height=\"20\" />\n                                                    <span class=\"fw-semibold me-auto\">Velzon</span>\n                                                    <small>11 mins ago</small>\n                                                    <button type=\"button\" class=\"btn-close\" data-bs-dismiss=\"toast\"\n                                                        aria-label=\"Close\"></button>\n                                                </div>\n                                                <div class=\"toast-body\">Hello, world! This is a toast message.</div>\n                                            </div>\n                                        </div>\n                                    </div>\n                                </b-col>\n\n                                <b-col xxl=\"6\">\n                                    <div>\n                                        <h5 class=\"fs-14\">Stacking</h5>\n                                        <p class=\"text-muted\">\n                                            When you have multiple toasts, we default to\n                                            vertically stacking\n                                            them in a readable manner.\n                                        </p>\n\n                                        <div class=\"p-3 bg-light\">\n                                            <!-- Toasts Stacking -->\n                                            <div class=\"toast fade show\" role=\"alert\" aria-live=\"assertive\"\n                                                data-bs-autohide=\"false\" aria-atomic=\"true\">\n                                                <div class=\"toast-header\">\n                                                    <img src=\"@/assets/images/logo-sm.png\" class=\"rounded me-2\"\n                                                        alt=\"...\" height=\"20\" />\n                                                    <span class=\"fw-semibold me-auto\">Velzon</span>\n                                                    <small>Just now</small>\n                                                    <button type=\"button\" class=\"btn-close\" data-bs-dismiss=\"toast\"\n                                                        aria-label=\"Close\"></button>\n                                                </div>\n                                                <div class=\"toast-body\">See? Just like this.</div>\n                                            </div>\n\n                                            <div class=\"toast fade show\" role=\"alert\" aria-live=\"assertive\"\n                                                data-bs-autohide=\"false\" aria-atomic=\"true\">\n                                                <div class=\"toast-header\">\n                                                    <img src=\"@/assets/images/logo-sm.png\" class=\"rounded me-2\"\n                                                        alt=\"...\" height=\"20\" />\n                                                    <span class=\"fw-semibold me-auto\">Velzon</span>\n                                                    <small>2 seconds ago</small>\n                                                    <button type=\"button\" class=\"btn-close\" data-bs-dismiss=\"toast\"\n                                                        aria-label=\"Close\"></button>\n                                                </div>\n                                                <div class=\"toast-body\">Heads up, toasts will stack automatically</div>\n                                            </div>\n                                        </div>\n                                    </div>\n                                </b-col>\n                            </b-row>\n\n                            <div class=\"mt-5\">\n                                <h5 class=\"fs-14\">Placement</h5>\n                                <b-row class=\"g-3\">\n                                    <b-col xxl=\"6\">\n                                        <div>\n                                            <p class=\"text-muted\">\n                                                Place toasts with custom CSS as you need them.\n                                                The top right\n                                                is often used\n                                                for notifications, as is the top middle. If youâ€™re only ever\n                                                going to show one toast\n                                                at a time, put\n                                                the positioning styles right on the\n                                                <code>.toast</code>.\n                                            </p>\n\n                                            <!-- Toasts Placement -->\n                                            <div class=\"bg-light\" aria-live=\"polite\" aria-atomic=\"true\"\n                                                style=\"position: relative; min-height: 200px;\">\n                                                <div class=\"toast fade show\" role=\"alert\" aria-live=\"assertive\"\n                                                    aria-atomic=\"true\" data-bs-toggle=\"toast\"\n                                                    style=\"position: absolute; top: 16px; right: 16px;\">\n                                                    <div class=\"toast-header\">\n                                                        <img src=\"@/assets/images/logo-sm.png\" class=\"rounded me-2\"\n                                                            alt=\"...\" height=\"20\" />\n                                                        <span class=\"fw-semibold me-auto\">Velzon</span>\n                                                        <small>06 mins ago</small>\n                                                        <button type=\"button\" class=\"btn-close\" data-bs-dismiss=\"toast\"\n                                                            aria-label=\"Close\"></button>\n                                                    </div>\n                                                    <div class=\"toast-body\">Hello, world! This is a toast message.</div>\n                                                </div>\n                                            </div>\n                                        </div>\n                                    </b-col>\n\n                                    <b-col xxl=\"6\">\n                                        <div>\n                                            <p class=\"text-muted\">\n                                                You can also get fancy with flexbox utilities\n                                                to align toasts\n                                                horizontally\n                                                and/or vertically.\n                                            </p>\n\n                                            <!-- Flexbox container for aligning the toasts -->\n                                            <div aria-live=\"polite\" aria-atomic=\"true\"\n                                                class=\"bg-light d-flex justify-content-center align-items-center\"\n                                                style=\"height: 200px;\">\n                                                <div class=\"toast fade show\" role=\"alert\" aria-live=\"assertive\"\n                                                    data-bs-autohide=\"false\" aria-atomic=\"true\">\n                                                    <div class=\"toast-header\">\n                                                        <img src=\"@/assets/images/logo-sm.png\" class=\"rounded me-2\"\n                                                            alt=\"...\" height=\"20\" />\n                                                        <span class=\"fw-semibold me-auto\">Velzon</span>\n                                                        <small>11 mins ago</small>\n                                                        <button type=\"button\" class=\"btn-close\" data-bs-dismiss=\"toast\"\n                                                            aria-label=\"Close\"></button>\n                                                    </div>\n                                                    <div class=\"toast-body\">Hello, world! This is a toast message.</div>\n                                                </div>\n                                            </div>\n                                        </div>\n                                    </b-col>\n                                </b-row>\n                            </div>\n                        </div>\n\n                        <div class=\"d-none code-view\">\n                            <pre class=\"language-markup\" style=\"height: 275px;\">\n<code>&lt;!-- Default Toast --&gt; \n&lt;div class=&quot;toast fade show&quot; role=&quot;alert&quot; aria-live=&quot;assertive&quot;\n    data-bs-autohide=&quot;false&quot; aria-atomic=&quot;true&quot;&gt;\n    &lt;div class=&quot;toast-header&quot;&gt;\n        &lt;img src=&quot;assets/images/logo-sm.png&quot; class=&quot;rounded me-2&quot;\n            alt=&quot;...&quot; height=&quot;20&quot;&gt;\n        &lt;span class=&quot;fw-semibold me-auto&quot;&gt;Velzon&lt;/span&gt;\n        &lt;small&gt;06 mins ago&lt;/small&gt;\n        &lt;button type=&quot;button&quot; class=&quot;btn-close&quot;\n            data-bs-dismiss=&quot;toast&quot; aria-label=&quot;Close&quot;&gt;&lt;/button&gt;\n    &lt;/div&gt;\n    &lt;div class=&quot;toast-body&quot;&gt;\n        Hello, world! This is a toast message.\n    &lt;/div&gt;\n&lt;/div&gt;</code>\n\n<code>&lt;!-- Translucent Toast --&gt; \n&lt;div class=&quot;p-3 bg-light&quot;&gt;\n    &lt;div class=&quot;toast fade show&quot; role=&quot;alert&quot; aria-live=&quot;assertive&quot;\n        data-bs-autohide=&quot;false&quot; aria-atomic=&quot;true&quot;&gt;\n        &lt;div class=&quot;toast-header&quot;&gt;\n            &lt;img src=&quot;assets/images/logo-sm.png&quot; class=&quot;rounded me-2&quot;\n                alt=&quot;...&quot; height=&quot;20&quot;&gt;\n            &lt;span class=&quot;fw-semibold me-auto&quot;&gt;Velzon&lt;/span&gt;\n            &lt;small&gt;11 mins ago&lt;/small&gt;\n            &lt;button type=&quot;button&quot; class=&quot;btn-close&quot;\n                data-bs-dismiss=&quot;toast&quot; aria-label=&quot;Close&quot;&gt;&lt;/button&gt;\n        &lt;/div&gt;\n        &lt;div class=&quot;toast-body&quot;&gt;\n            Hello, world! This is a toast message.\n        &lt;/div&gt;\n    &lt;/div&gt;\n&lt;/div&gt;</code>\n\n<code>&lt;!-- Stacking Toast --&gt; \n&lt;div class=&quot;toast fade show&quot; role=&quot;alert&quot; aria-live=&quot;assertive&quot;\n    data-bs-autohide=&quot;false&quot; aria-atomic=&quot;true&quot;&gt;\n    &lt;div class=&quot;toast-header&quot;&gt;\n        &lt;img src=&quot;assets/images/logo-sm.png&quot; class=&quot;rounded me-2&quot;\n            alt=&quot;...&quot; height=&quot;20&quot;&gt;\n        &lt;span class=&quot;fw-semibold me-auto&quot;&gt;Velzon&lt;/span&gt;\n        &lt;small&gt;Just now&lt;/small&gt;\n        &lt;button type=&quot;button&quot; class=&quot;btn-close&quot;\n            data-bs-dismiss=&quot;toast&quot; aria-label=&quot;Close&quot;&gt;&lt;/button&gt;\n    &lt;/div&gt;\n    &lt;div class=&quot;toast-body&quot;&gt;\n        See? Just like this.\n    &lt;/div&gt;\n&lt;/div&gt;</code>\n\n<code>&lt;!-- Placement Toast --&gt; \n&lt;div class=&quot;bg-light&quot; aria-live=&quot;polite&quot; aria-atomic=&quot;true&quot;\n    style=&quot;position: relative; min-height: 200px;&quot;&gt;\n    &lt;div class=&quot;toast fade show&quot; role=&quot;alert&quot; aria-live=&quot;assertive&quot;\n        aria-atomic=&quot;true&quot; data-bs-toggle=&quot;toast&quot;\n        style=&quot;position: absolute; top: 16px; right: 16px;&quot;&gt;\n        &lt;div class=&quot;toast-header&quot;&gt;\n            &lt;img src=&quot;assets/images/logo-sm.png&quot;\n                class=&quot;rounded me-2&quot; alt=&quot;...&quot; height=&quot;20&quot;&gt;\n            &lt;span class=&quot;fw-semibold me-auto&quot;&gt;Velzon&lt;/span&gt;\n            &lt;small&gt;06 mins ago&lt;/small&gt;\n            &lt;button type=&quot;button&quot; class=&quot;btn-close&quot;\n                data-bs-dismiss=&quot;toast&quot; aria-label=&quot;Close&quot;&gt;&lt;/button&gt;\n        &lt;/div&gt;\n        &lt;div class=&quot;toast-body&quot;&gt;\n            Hello, world! This is a toast message.\n        &lt;/div&gt;\n    &lt;/div&gt;\n&lt;/div&gt;</code>\n\n<code> &lt;!-- Flexbox container for aligning the toasts --&gt;\n&lt;div aria-live=&quot;polite&quot; aria-atomic=&quot;true&quot;\n    class=&quot;bg-light d-flex justify-content-center align-items-center&quot;\n    style=&quot;height: 200px;&quot;&gt;\n    &lt;div class=&quot;toast fade show&quot; role=&quot;alert&quot; aria-live=&quot;assertive&quot;\n        data-bs-autohide=&quot;false&quot; aria-atomic=&quot;true&quot;&gt;\n        &lt;div class=&quot;toast-header&quot;&gt;\n            &lt;img src=&quot;assets/images/logo-sm.png&quot;\n                class=&quot;rounded me-2&quot; alt=&quot;...&quot; height=&quot;20&quot;&gt;\n            &lt;span class=&quot;fw-semibold me-auto&quot;&gt;Velzon&lt;/span&gt;\n            &lt;small&gt;11 mins ago&lt;/small&gt;\n            &lt;button type=&quot;button&quot; class=&quot;btn-close&quot;\n                data-bs-dismiss=&quot;toast&quot; aria-label=&quot;Close&quot;&gt;&lt;/button&gt;\n        &lt;/div&gt;\n        &lt;div class=&quot;toast-body&quot;&gt;\n            Hello, world! This is a toast message.\n        &lt;/div&gt;\n    &lt;/div&gt;\n&lt;/div&gt;</code></pre>\n                        </div>\n                    </b-card-body>\n                </b-card>\n            </b-col>\n        </b-row>\n    </Layout>\n</template>"],"mappings":"AACA,OAAOA,MAAK,MAAO,wBAAwB;AAC3C,OAAOC,UAAS,MAAO,0BAA0B;AAEjD,OAAO,SAAS;AAChB,OAAO,0BAA0B;AAEjC,eAAe;EACXC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,KAAK,EAAE,eAAe;MACtBC,KAAK,EAAE,CACH;QACIC,IAAI,EAAE,SAAS;QACfC,IAAI,EAAE;MACV,CAAC,EACD;QACID,IAAI,EAAE,eAAe;QACrBE,MAAM,EAAE;MACZ,CAAC;IAET,CAAC;EACL,CAAC;EACDC,IAAI,EAAE,eAAe;EACrBC,UAAU,EAAE;IACRT,MAAM;IACNC;EACJ,CAAC;EACDS,OAAOA,CAAA,EAAG;IACN,MAAMC,QAAO,GAAIC,QAAQ,CAACC,sBAAsB,CAAC,eAAe,CAAC;IACjEC,KAAK,CAACC,IAAI,CAACJ,QAAQ,CAAC,CAACK,OAAO,CAAEC,KAAK,IAAK;MACpCA,KAAK,CAACC,gBAAgB,CAAC,QAAQ,EAAE,MAAM;QACnC,MAAMC,IAAG,GAAIF,KAAK,CAACG,OAAO,CAAC,OAAO,CAAC;QACnC,MAAMC,OAAM,GAAIF,IAAI,CAACG,aAAa,CAAC,eAAe,CAAC;QACnD,MAAMC,IAAG,GAAIJ,IAAI,CAACG,aAAa,CAAC,YAAY,CAAC;QAC7C,IAAIL,KAAK,CAACO,OAAO,EAAE;UACf;UACAH,OAAO,CAACI,SAAS,CAACC,GAAG,CAAC,QAAQ,CAAC;UAC/BH,IAAI,CAACE,SAAS,CAACE,MAAM,CAAC,QAAQ,CAAC;QACnC,OAAO;UACH;UACAN,OAAO,CAACI,SAAS,CAACE,MAAM,CAAC,QAAQ,CAAC;UAClCJ,IAAI,CAACE,SAAS,CAACC,GAAG,CAAC,QAAQ,CAAC;QAChC;MACJ,CAAC,CAAC;IACN,CAAC,CAAC;EACN;AACJ,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}