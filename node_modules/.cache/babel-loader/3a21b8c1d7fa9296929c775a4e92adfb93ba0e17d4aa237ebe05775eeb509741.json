{"ast":null,"code":"import Layout from \"../../../layouts/main.vue\";\nimport appConfig from \"../../../../app.config\";\nimport PageHeader from \"@/components/page-header\";\nimport animationData from \"@/components/widgets/lupuorrc.json\";\nimport Lottie from \"@/components/widgets/lottie.vue\";\nexport default {\n  page: {\n    title: \"Checkout\",\n    meta: [{\n      name: \"description\",\n      content: appConfig.description\n    }]\n  },\n  data() {\n    return {\n      title: \"Checkout\",\n      items: [{\n        text: \"Ecommerce\",\n        href: \"/\"\n      }, {\n        text: \"Checkout\",\n        active: true\n      }],\n      defaultOptions: {\n        animationData: animationData\n      }\n    };\n  },\n  components: {\n    Layout,\n    PageHeader,\n    lottie: Lottie\n  },\n  mounted() {\n    document.querySelectorAll(\".checkout-tab\").forEach(form => {\n      // next tab\n      form.querySelectorAll(\".nexttab\").forEach(nextButton => {\n        var tabEl = form.querySelectorAll('button[data-bs-toggle=\"pill\"]');\n        tabEl.forEach(item => {\n          item.addEventListener(\"show.bs.tab\", event => {\n            event.target.classList.add(\"done\");\n          });\n        });\n        nextButton.addEventListener(\"click\", () => {\n          var nextTab = nextButton.getAttribute(\"data-nexttab\");\n          document.getElementById(nextTab).click();\n        });\n      });\n\n      //Pervies tab\n      form.querySelectorAll(\".previestab\").forEach(prevButton => {\n        prevButton.addEventListener(\"click\", () => {\n          var prevTab = prevButton.getAttribute(\"data-previous\");\n          var totalDone = prevButton.closest(\"form\").querySelectorAll(\".custom-nav .done\").length;\n          for (var i = totalDone - 1; i < totalDone; i++) {\n            prevButton.closest(\"form\").querySelectorAll(\".custom-nav .done\")[i] ? prevButton.closest(\"form\").querySelectorAll(\".custom-nav .done\")[i].classList.remove(\"done\") : \"\";\n          }\n          document.getElementById(prevTab).click();\n        });\n      });\n\n      // Step number click\n      var tabButtons = form.querySelectorAll('button[data-bs-toggle=\"pill\"]');\n      tabButtons.forEach((button, i) => {\n        button.setAttribute(\"data-position\", i);\n        button.addEventListener(\"click\", () => {\n          form.querySelectorAll(\".custom-nav .done\").length > 0 ? form.querySelectorAll(\".custom-nav .done\").forEach(doneTab => {\n            doneTab.classList.remove(\"done\");\n          }) : \"\";\n          for (var j = 0; j <= i; j++) {\n            tabButtons[j].classList.contains(\"active\") ? tabButtons[j].classList.remove(\"done\") : tabButtons[j].classList.add(\"done\");\n          }\n        });\n      });\n    });\n  }\n};","map":{"version":3,"names":["Layout","appConfig","PageHeader","animationData","Lottie","page","title","meta","name","content","description","data","items","text","href","active","defaultOptions","components","lottie","mounted","document","querySelectorAll","forEach","form","nextButton","tabEl","item","addEventListener","event","target","classList","add","nextTab","getAttribute","getElementById","click","prevButton","prevTab","totalDone","closest","length","i","remove","tabButtons","button","setAttribute","doneTab","j","contains"],"sources":["C:\\Users\\UMIDJON\\Desktop\\default2\\src\\views\\apps\\ecommerce\\checkout.vue"],"sourcesContent":["<script>\nimport Layout from \"../../../layouts/main.vue\";\nimport appConfig from \"../../../../app.config\";\nimport PageHeader from \"@/components/page-header\";\n\nimport animationData from \"@/components/widgets/lupuorrc.json\";\nimport Lottie from \"@/components/widgets/lottie.vue\";\n\nexport default {\n  page: {\n    title: \"Checkout\",\n    meta: [\n      {\n        name: \"description\",\n        content: appConfig.description,\n      },\n    ],\n  },\n  data() {\n    return {\n      title: \"Checkout\",\n      items: [\n        {\n          text: \"Ecommerce\",\n          href: \"/\",\n        },\n        {\n          text: \"Checkout\",\n          active: true,\n        },\n      ],\n      defaultOptions: { animationData: animationData },\n    };\n  },\n  components: {\n    Layout,\n    PageHeader,\n    lottie: Lottie,\n  },\n  mounted() {\n    document.querySelectorAll(\".checkout-tab\").forEach((form) => {\n      // next tab\n      form.querySelectorAll(\".nexttab\").forEach((nextButton) => {\n        var tabEl = form.querySelectorAll('button[data-bs-toggle=\"pill\"]');\n        tabEl.forEach((item) => {\n          item.addEventListener(\"show.bs.tab\", (event) => {\n            event.target.classList.add(\"done\");\n          });\n        });\n        nextButton.addEventListener(\"click\", () => {\n          var nextTab = nextButton.getAttribute(\"data-nexttab\");\n          document.getElementById(nextTab).click();\n        });\n      });\n\n      //Pervies tab\n      form.querySelectorAll(\".previestab\").forEach((prevButton) => {\n        prevButton.addEventListener(\"click\", () => {\n          var prevTab = prevButton.getAttribute(\"data-previous\");\n          var totalDone = prevButton\n            .closest(\"form\")\n            .querySelectorAll(\".custom-nav .done\").length;\n          for (var i = totalDone - 1; i < totalDone; i++) {\n            prevButton.closest(\"form\").querySelectorAll(\".custom-nav .done\")[i]\n              ? prevButton.closest(\"form\").querySelectorAll(\".custom-nav .done\")[i].classList.remove(\"done\")\n              : \"\";\n          }\n          document.getElementById(prevTab).click();\n        });\n      });\n\n      // Step number click\n      var tabButtons = form.querySelectorAll('button[data-bs-toggle=\"pill\"]');\n      tabButtons.forEach((button, i) => {\n        button.setAttribute(\"data-position\", i);\n        button.addEventListener(\"click\", () => {\n          form.querySelectorAll(\".custom-nav .done\").length > 0\n            ? form\n              .querySelectorAll(\".custom-nav .done\")\n              .forEach((doneTab) => {\n                doneTab.classList.remove(\"done\");\n              })\n            : \"\";\n          for (var j = 0; j <= i; j++) {\n            tabButtons[j].classList.contains(\"active\")\n              ? tabButtons[j].classList.remove(\"done\")\n              : tabButtons[j].classList.add(\"done\");\n          }\n        });\n      });\n    });\n  },\n};\n</script>\n\n<template>\n  <Layout>\n    <PageHeader :title=\"title\" :items=\"items\" />\n    <b-row>\n      <b-col xl=\"8\">\n        <b-card no-body>\n          <b-card-body class=\"checkout-tab\">\n            <b-form action=\"#\">\n              <div class=\"step-arrow-nav mt-n3 mx-n3 mb-3\">\n                <ul class=\"nav nav-pills nav-justified custom-nav\" role=\"tablist\">\n                  <li class=\"nav-item\" role=\"presentation\">\n                    <button class=\"nav-link fs-15 p-3 active\" id=\"pills-bill-info-tab\" data-bs-toggle=\"pill\"\n                      data-bs-target=\"#pills-bill-info\" type=\"button\" role=\"tab\" aria-controls=\"pills-bill-info\"\n                      aria-selected=\"true\">\n                      <i class=\"\n                          ri-user-2-line\n                          fs-16\n                          avatar-xs d-inline-flex align-items-center justify-content-center\n                          bg-soft-primary\n                          text-primary\n                          rounded-circle\n                          align-middle\n                          me-2\n                        \"></i>\n                      Personal Info\n                    </button>\n                  </li>\n                  <li class=\"nav-item\" role=\"presentation\">\n                    <button class=\"nav-link fs-15 p-3\" id=\"pills-bill-address-tab\" data-bs-toggle=\"pill\"\n                      data-bs-target=\"#pills-bill-address\" type=\"button\" role=\"tab\" aria-controls=\"pills-bill-address\"\n                      aria-selected=\"false\">\n                      <i class=\"\n                          ri-truck-line\n                          fs-16\n                          avatar-xs d-inline-flex align-items-center justify-content-center\n                          bg-soft-primary\n                          text-primary\n                          rounded-circle\n                          align-middle\n                          me-2\n                        \"></i>\n                      Shipping Info\n                    </button>\n                  </li>\n                  <li class=\"nav-item\" role=\"presentation\">\n                    <button class=\"nav-link fs-15 p-3\" id=\"pills-payment-tab\" data-bs-toggle=\"pill\"\n                      data-bs-target=\"#pills-payment\" type=\"button\" role=\"tab\" aria-controls=\"pills-payment\"\n                      aria-selected=\"false\">\n                      <i class=\"\n                          ri-bank-card-line\n                          fs-16\n                          avatar-xs d-inline-flex align-items-center justify-content-center\n                          bg-soft-primary\n                          text-primary\n                          rounded-circle\n                          align-middle\n                          me-2\n                        \"></i>\n                      Payment Info\n                    </button>\n                  </li>\n                  <li class=\"nav-item\" role=\"presentation\">\n                    <button class=\"nav-link fs-15 p-3\" id=\"pills-finish-tab\" data-bs-toggle=\"pill\"\n                      data-bs-target=\"#pills-finish\" type=\"button\" role=\"tab\" aria-controls=\"pills-finish\"\n                      aria-selected=\"false\">\n                      <i class=\"\n                          ri-checkbox-circle-line\n                          fs-16\n                          avatar-xs d-inline-flex align-items-center justify-content-center\n                          bg-soft-primary\n                          text-primary\n                          rounded-circle\n                          align-middle\n                          me-2\n                        \"></i>Finish\n                    </button>\n                  </li>\n                </ul>\n              </div>\n\n              <div class=\"tab-content\">\n                <div class=\"tab-pane fade show active\" id=\"pills-bill-info\" role=\"tabpanel\"\n                  aria-labelledby=\"pills-bill-info-tab\">\n                  <div>\n                    <h5 class=\"mb-1\">Billing Information</h5>\n                    <p class=\"text-muted mb-4\">\n                      Please fill all information below\n                    </p>\n                  </div>\n\n                  <div>\n                    <b-row>\n                      <b-col sm=\"6\">\n                        <div class=\"mb-3\">\n                          <label for=\"billinginfo-firstName\" class=\"form-label\">First Name</label>\n                          <input type=\"text\" class=\"form-control\" id=\"billinginfo-firstName\"\n                            placeholder=\"Enter first name\" value=\"\" />\n                        </div>\n                      </b-col>\n\n                      <b-col sm=\"6\">\n                        <div class=\"mb-3\">\n                          <label for=\"billinginfo-lastName\" class=\"form-label\">Last Name</label>\n                          <input type=\"text\" class=\"form-control\" id=\"billinginfo-lastName\"\n                            placeholder=\"Enter last name\" value=\"\" />\n                        </div>\n                      </b-col>\n                    </b-row>\n\n                    <b-row>\n                      <b-col sm=\"6\">\n                        <div class=\"mb-3\">\n                          <label for=\"billinginfo-email\" class=\"form-label\">Email\n                            <span class=\"text-muted\">(Optional)</span></label>\n                          <input type=\"email\" class=\"form-control\" id=\"billinginfo-email\" placeholder=\"Enter email\" />\n                        </div>\n                      </b-col>\n\n                      <b-col sm=\"6\">\n                        <div class=\"mb-3\">\n                          <label for=\"billinginfo-phone\" class=\"form-label\">Phone</label>\n                          <input type=\"text\" class=\"form-control\" id=\"billinginfo-phone\"\n                            placeholder=\"Enter phone no.\" />\n                        </div>\n                      </b-col>\n                    </b-row>\n\n                    <div class=\"mb-3\">\n                      <label for=\"billinginfo-address\" class=\"form-label\">Address</label>\n                      <textarea class=\"form-control\" id=\"billinginfo-address\" placeholder=\"Enter address\"\n                        rows=\"3\"></textarea>\n                    </div>\n\n                    <b-row>\n                      <b-col md=\"4\">\n                        <div class=\"mb-3\">\n                          <label for=\"country\" class=\"form-label\">Country</label>\n                          <select class=\"form-select\" id=\"country\" data-plugin=\"choices\">\n                            <option value=\"\">Select Country...</option>\n                            <option selected>United States</option>\n                          </select>\n                        </div>\n                      </b-col>\n\n                      <b-col md=\"4\">\n                        <div class=\"mb-3\">\n                          <label for=\"state\" class=\"form-label\">State</label>\n                          <select class=\"form-select\" id=\"state\" data-plugin=\"choices\">\n                            <option value=\"\">Select State...</option>\n                            <option value=\"Alabama\">Alabama</option>\n                            <option value=\"Alaska\">Alaska</option>\n                            <option value=\"American Samoa\">\n                              American Samoa\n                            </option>\n                            <option value=\"California\" selected>\n                              California\n                            </option>\n                            <option value=\"Colorado\">Colorado</option>\n                            <option value=\"District Of Columbia\">\n                              District Of Columbia\n                            </option>\n                            <option value=\"Florida\">Florida</option>\n                            <option value=\"Georgia\">Georgia</option>\n                            <option value=\"Guam\">Guam</option>\n                            <option value=\"Hawaii\">Hawaii</option>\n                            <option value=\"Idaho\">Idaho</option>\n                            <option value=\"Kansas\">Kansas</option>\n                            <option value=\"Louisiana\">Louisiana</option>\n                            <option value=\"Montana\">Montana</option>\n                            <option value=\"Nevada\">Nevada</option>\n                            <option value=\"New Jersey\">New Jersey</option>\n                            <option value=\"New Mexico\">New Mexico</option>\n                            <option value=\"New York\">New York</option>\n                          </select>\n                        </div>\n                      </b-col>\n\n                      <b-col md=\"4\">\n                        <div class=\"mb-3\">\n                          <label for=\"zip\" class=\"form-label\">Zip Code</label>\n                          <input type=\"text\" class=\"form-control\" id=\"zip\" placeholder=\"Enter zip code\" />\n                        </div>\n                      </b-col>\n                    </b-row>\n\n                    <div class=\"d-flex align-items-start gap-3 mt-3\">\n                      <b-button variant=\"primary\" type=\"button\" class=\"btn-label right ms-auto nexttab\"\n                        data-nexttab=\"pills-bill-address-tab\">\n                        <i class=\"\n                            ri-truck-line\n                            label-icon\n                            align-middle\n                            fs-16\n                            ms-2\n                          \"></i>Proceed to Shipping\n                      </b-button>\n                    </div>\n                  </div>\n                </div>\n\n                <div class=\"tab-pane fade\" id=\"pills-bill-address\" role=\"tabpanel\"\n                  aria-labelledby=\"pills-bill-address-tab\">\n                  <div>\n                    <h5 class=\"mb-1\">Shipping Information</h5>\n                    <p class=\"text-muted mb-4\">\n                      Please fill all information below\n                    </p>\n                  </div>\n\n                  <div class=\"mt-4\">\n                    <div class=\"d-flex align-items-center mb-2\">\n                      <div class=\"flex-grow-1\">\n                        <h5 class=\"fs-14 mb-0\">Saved Address</h5>\n                      </div>\n                      <div class=\"flex-shrink-0\">\n                        <b-button type=\"button\" variant=\"success\" size=\"sm\" class=\"mb-3\">\n                          Add Address\n                        </b-button>\n                      </div>\n                    </div>\n                    <b-row class=\"gy-3\">\n                      <b-col lg=\"4\" sm=\"6\">\n                        <div class=\"form-check card-radio\">\n                          <input id=\"shippingAddress01\" name=\"shippingAddress\" type=\"radio\" class=\"form-check-input\"\n                            checked />\n                          <label class=\"form-check-label\" for=\"shippingAddress01\">\n                            <span class=\"\n                                mb-4\n                                fw-semibold\n                                d-block\n                                text-muted text-uppercase\n                              \">Home Address</span>\n\n                            <span class=\"fs-14 mb-2 d-block\">Marcus Alfaro</span>\n                            <span class=\"\n                                text-muted\n                                fw-normal\n                                text-wrap\n                                mb-1\n                                d-block\n                              \">4739 Bubby Drive Austin, TX 78729</span>\n                            <span class=\"text-muted fw-normal d-block\">Mo. 012-345-6789</span>\n                          </label>\n                        </div>\n                        <div class=\"\n                            d-flex\n                            flex-wrap\n                            p-2\n                            py-1\n                            bg-light\n                            rounded-bottom\n                            border\n                            mt-n1\n                          \">\n                          <div>\n                            <b-link href=\"#\" class=\"d-block text-body p-1 px-2\">\n                              <i class=\"ri-pencil-fill text-muted align-bottom me-1\"></i>\n                              Edit\n                            </b-link>\n                          </div>\n                          <div>\n                            <b-link href=\"#\" class=\"d-block text-body p-1 px-2\">\n                              <i class=\"ri-delete-bin-fill text-muted align-bottom me-1\"></i>\n                              Remove\n                            </b-link>\n                          </div>\n                        </div>\n                      </b-col>\n                      <b-col lg=\"4\" sm=\"6\">\n                        <div class=\"form-check card-radio\">\n                          <input id=\"shippingAddress02\" name=\"shippingAddress\" type=\"radio\" class=\"form-check-input\" />\n                          <label class=\"form-check-label\" for=\"shippingAddress02\">\n                            <span class=\"\n                                mb-4\n                                fw-semibold\n                                d-block\n                                text-muted text-uppercase\n                              \">Office Address</span>\n\n                            <span class=\"fs-14 mb-2 d-block\">James Honda</span>\n                            <span class=\"\n                                text-muted\n                                fw-normal\n                                text-wrap\n                                mb-1\n                                d-block\n                              \">1246 Virgil Street Pensacola, FL 32501\n                            </span>\n                            <span class=\"text-muted fw-normal d-block\">Mo. 012-345-6789</span>\n                          </label>\n                        </div>\n                        <div class=\"\n                            d-flex\n                            flex-wrap\n                            p-2\n                            py-1\n                            bg-light\n                            rounded-bottom\n                            border\n                            mt-n1\n                          \">\n                          <div>\n                            <b-link href=\"#\" class=\"d-block text-body p-1 px-2\">\n                              <i class=\" ri-pencil-fill text-muted align-bottom me-1\"></i>\n                              Edit\n                            </b-link>\n                          </div>\n                          <div>\n                            <b-link href=\"#\" class=\"d-block text-body p-1 px-2\"><i\n                                class=\"ri-delete-bin-fill text-muted align-bottom me-1\"></i>\n                              Remove</b-link>\n                          </div>\n                        </div>\n                      </b-col>\n                    </b-row>\n\n                    <div class=\"mt-4\">\n                      <h5 class=\"fs-14 mb-3\">Shipping Method</h5>\n\n                      <b-row class=\"g-4\">\n                        <b-col lg=\"6\">\n                          <div class=\"form-check card-radio\">\n                            <input id=\"shippingMethod01\" name=\"shippingMethod\" type=\"radio\" class=\"form-check-input\"\n                              checked />\n                            <label class=\"form-check-label\" for=\"shippingMethod01\">\n                              <span class=\"\n                                  fs-20\n                                  float-end\n                                  mt-2\n                                  text-wrap\n                                  d-block\n                                  fw-semibold\n                                \">Free</span>\n                              <span class=\"fs-14 mb-1 text-wrap d-block\">Free Delivery</span>\n                              <span class=\"text-muted fw-normal text-wrap d-block\">Expected Delivery 3 to 5 Days</span>\n                            </label>\n                          </div>\n                        </b-col>\n                        <b-col lg=\"6\">\n                          <div class=\"form-check card-radio\">\n                            <input id=\"shippingMethod02\" name=\"shippingMethod\" type=\"radio\" class=\"form-check-input\"\n                              checked />\n                            <label class=\"form-check-label\" for=\"shippingMethod02\">\n                              <span class=\"\n                                  fs-20\n                                  float-end\n                                  mt-2\n                                  text-wrap\n                                  d-block\n                                  fw-semibold\n                                \">$24.99</span>\n                              <span class=\"fs-14 mb-1 text-wrap d-block\">Express Delivery</span>\n                              <span class=\"text-muted fw-normal text-wrap d-block\">Delivery within 24hrs.</span>\n                            </label>\n                          </div>\n                        </b-col>\n                      </b-row>\n                    </div>\n                  </div>\n\n                  <div class=\"d-flex align-items-start gap-3 mt-4\">\n                    <b-button type=\"button\" variant=\"light\" class=\"btn-label previestab\"\n                      data-previous=\"pills-bill-info-tab\">\n                      <i class=\"\n                          ri-arrow-left-line\n                          label-icon\n                          align-middle\n                          fs-16\n                          me-2\n                        \"></i>Back to Personal Info\n                    </b-button>\n                    <b-button type=\"button\" variant=\"primary\" class=\"btn-label right ms-auto nexttab\"\n                      data-nexttab=\"pills-payment-tab\">\n                      <i class=\"\n                          ri-bank-card-line\n                          label-icon\n                          align-middle\n                          fs-16\n                          ms-2\n                        \"></i>Continue to Payment\n                    </b-button>\n                  </div>\n                </div>\n\n                <div class=\"tab-pane fade\" id=\"pills-payment\" role=\"tabpanel\" aria-labelledby=\"pills-payment-tab\">\n                  <div>\n                    <h5 class=\"mb-1\">Payment Selection</h5>\n                    <p class=\"text-muted mb-4\">\n                      Please select and enter your billing information\n                    </p>\n                  </div>\n\n                  <b-row class=\"g-4\">\n                    <b-col lg=\"4\" sm=\"6\">\n                      <div data-bs-toggle=\"collapse\" data-bs-target=\"#paymentmethodCollapse.show\" aria-expanded=\"false\"\n                        aria-controls=\"paymentmethodCollapse\">\n                        <div class=\"form-check card-radio\">\n                          <input id=\"paymentMethod01\" name=\"paymentMethod\" type=\"radio\" class=\"form-check-input\" />\n                          <label class=\"form-check-label\" for=\"paymentMethod01\">\n                            <span class=\"fs-16 text-muted me-2\"><i class=\"ri-paypal-fill align-bottom\"></i></span>\n                            <span class=\"fs-14 text-wrap\">Paypal</span>\n                          </label>\n                        </div>\n                      </div>\n                    </b-col>\n                    <b-col lg=\"4\" sm=\"6\">\n                      <div data-bs-toggle=\"collapse\" data-bs-target=\"#paymentmethodCollapse\" aria-expanded=\"true\"\n                        aria-controls=\"paymentmethodCollapse\">\n                        <div class=\"form-check card-radio\">\n                          <input id=\"paymentMethod02\" name=\"paymentMethod\" type=\"radio\" class=\"form-check-input\"\n                            checked />\n                          <label class=\"form-check-label\" for=\"paymentMethod02\">\n                            <span class=\"fs-16 text-muted me-2\"><i class=\"ri-bank-card-fill align-bottom\"></i></span>\n                            <span class=\"fs-14 text-wrap\">Credit / Debit Card</span>\n                          </label>\n                        </div>\n                      </div>\n                    </b-col>\n\n                    <b-col lg=\"4\" sm=\"6\">\n                      <div data-bs-toggle=\"collapse\" data-bs-target=\"#paymentmethodCollapse.show\" aria-expanded=\"false\"\n                        aria-controls=\"paymentmethodCollapse\">\n                        <div class=\"form-check