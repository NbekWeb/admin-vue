{"ast":null,"code":"import Slider from \"@vueform/slider\";\nimport Layout from \"../../layouts/main.vue\";\nimport PageHeader from \"@/components/page-header\";\nimport appConfig from \"../../../app.config\";\nexport default {\n  page: {\n    title: \"Range Slider\",\n    meta: [{\n      name: \"description\",\n      content: appConfig.description\n    }]\n  },\n  data() {\n    return {\n      title: \"Range Slider\",\n      items: [{\n        text: \"Forms\",\n        href: \"/\"\n      }, {\n        text: \"Range Slider\",\n        active: true\n      }],\n      simpleValue: 10,\n      sliderCustomzie: 300,\n      sliderWithLabel: {\n        value: 45,\n        data: [15, 30, 45, 60, 75, 90, 120],\n        range: [{\n          label: \"15 mins\"\n        }, {\n          label: \"30 mins\",\n          isHide: true\n        }, {\n          label: \"45 mins\"\n        }, {\n          label: \"1 hr\",\n          isHide: true\n        }, {\n          label: \"1 hr 15 mins\"\n        }, {\n          label: \"1 hr 30 mins\",\n          isHide: true\n        }, {\n          label: \"2 hrs\"\n        }],\n        rangeValue: {}\n      },\n      value: [20, 30, 40],\n      merge: 10,\n      format: {\n        prefix: \"$\",\n        decimals: 2\n      },\n      loader: null,\n      loadingValue: 0,\n      customStyle: 50,\n      stepData: 10,\n      rtlvalue: 50,\n      orientation: \"vertical\",\n      direction: \"rtl\"\n    };\n  },\n  components: {\n    Slider,\n    Layout,\n    PageHeader\n  },\n  methods: {\n    /**\n     * Range and label slider set range\n     */\n    callbackRange(val) {\n      this.sliderWithLabel.rangeValue = val;\n    },\n    /**\n     * Loading slider\n     */\n    startLoad() {\n      this.loader = setInterval(() => {\n        this.loadingValue++;\n        if (this.loadingValue === 100) {\n          clearInterval(this.loader);\n        }\n      }, 100);\n    }\n  }\n};","map":{"version":3,"names":["Slider","Layout","PageHeader","appConfig","page","title","meta","name","content","description","data","items","text","href","active","simpleValue","sliderCustomzie","sliderWithLabel","value","range","label","isHide","rangeValue","merge","format","prefix","decimals","loader","loadingValue","customStyle","stepData","rtlvalue","orientation","direction","components","methods","callbackRange","val","startLoad","setInterval","clearInterval"],"sources":["C:\\Users\\UMIDJON\\Desktop\\default2\\src\\views\\forms\\range-sliders.vue"],"sourcesContent":["<script>\nimport Slider from \"@vueform/slider\";\n\nimport Layout from \"../../layouts/main.vue\";\nimport PageHeader from \"@/components/page-header\";\nimport appConfig from \"../../../app.config\";\n\nexport default {\n  page: {\n    title: \"Range Slider\",\n    meta: [{\n      name: \"description\",\n      content: appConfig.description\n    }],\n  },\n  data() {\n    return {\n      title: \"Range Slider\",\n      items: [{\n        text: \"Forms\",\n        href: \"/\",\n      },\n      {\n        text: \"Range Slider\",\n        active: true,\n      },\n      ],\n      simpleValue: 10,\n      sliderCustomzie: 300,\n      sliderWithLabel: {\n        value: 45,\n        data: [15, 30, 45, 60, 75, 90, 120],\n        range: [{\n          label: \"15 mins\",\n        },\n        {\n          label: \"30 mins\",\n          isHide: true,\n        },\n        {\n          label: \"45 mins\",\n        },\n        {\n          label: \"1 hr\",\n          isHide: true,\n        },\n        {\n          label: \"1 hr 15 mins\",\n        },\n        {\n          label: \"1 hr 30 mins\",\n          isHide: true,\n        },\n        {\n          label: \"2 hrs\",\n        },\n        ],\n        rangeValue: {},\n      },\n      value: [20, 30, 40],\n      merge: 10,\n      format: {\n        prefix: \"$\",\n        decimals: 2,\n      },\n      loader: null,\n      loadingValue: 0,\n      customStyle: 50,\n      stepData: 10,\n      rtlvalue: 50,\n      orientation: \"vertical\",\n      direction: \"rtl\",\n    };\n  },\n  components: {\n    Slider,\n    Layout,\n    PageHeader,\n  },\n  methods: {\n    /**\n     * Range and label slider set range\n     */\n    callbackRange(val) {\n      this.sliderWithLabel.rangeValue = val;\n    },\n    /**\n     * Loading slider\n     */\n    startLoad() {\n      this.loader = setInterval(() => {\n        this.loadingValue++;\n        if (this.loadingValue === 100) {\n          clearInterval(this.loader);\n        }\n      }, 100);\n    },\n  },\n};\n</script>\n\n<template>\n  <Layout>\n    <PageHeader :title=\"title\" :items=\"items\" />\n    <b-row>\n      <b-col cols=\"12\">\n        <b-card no-body>\n          <b-card-body>\n            <b-card-title>Range slider</b-card-title>\n            <p class=\"card-title-desc\">\n              Cool, comfortable, responsive and easily customizable range slider\n            </p>\n            <b-row>\n              <b-col md=\"6\">\n                <div class=\"p-3\">\n                  <h5 class=\"font-size-14 mb-5 mt-0\">Default</h5>\n                  <div class=\"p-3\">\n                    <Slider v-model=\"simpleValue\" />\n                  </div>\n                </div>\n              </b-col>\n              <b-col md=\"6\">\n                <div class=\"p-3\">\n                  <h5 class=\"font-size-14 mb-5 mt-0\">Min-Max</h5>\n                  <div class=\"p-3\">\n                    <Slider v-model=\"sliderCustomzie\" :min=\"100\" :max=\"500\" />\n                  </div>\n                </div>\n              </b-col>\n            </b-row>\n            <b-row>\n              <b-col md=\"6\">\n                <div class=\"p-3\">\n                  <h5 class=\"font-size-14 mb-5 mt-0\">Range and Label</h5>\n                  <div class=\"p-3\">\n                    <Slider v-model=\"sliderWithLabel.value\" :data=\"sliderWithLabel.data\" :range=\"sliderWithLabel.range\"\n                      @callbackRange=\"callbackRange\" />\n                  </div>\n                </div>\n              </b-col>\n              <b-col md=\"6\">\n                <div class=\"p-3\">\n                  <h5 class=\"font-size-14 mb-5 mt-0\">Loading</h5>\n                  <div class=\"p-3\">\n                    <Slider v-model=\"loadingValue\" :is-disabled=\"true\" class=\"slider-red\" />\n                    <br />\n                    <b-button size=\"sm\" variant=\"light\" @click=\"startLoad\">Start</b-button>\n                  </div>\n                </div>\n              </b-col>\n            </b-row>\n            <b-row>\n              <b-col md=\"6\">\n                <div class=\"p-3\">\n                  <h5 class=\"font-size-14 mb-4 mt-0\">Rtl</h5>\n                  <div class=\"p-3\">\n                    <Slider v-model=\"rtlvalue\" :direction=\"direction\" :orientation=\"orientation\" />\n                  </div>\n                </div>\n              </b-col>\n              <b-col md=\"6\">\n                <div class=\"p-3\">\n                  <h5 class=\"font-size-14 mb-5 mt-0\">Format</h5>\n                  <div class=\"p-3\">\n                    <Slider v-model=\"value\" :merge=\"merge\" :format=\"format\" />\n                  </div>\n                </div>\n              </b-col>\n            </b-row>\n          </b-card-body>\n        </b-card>\n      </b-col>\n    </b-row>\n  </Layout>\n</template>"],"mappings":"AACA,OAAOA,MAAK,MAAO,iBAAiB;AAEpC,OAAOC,MAAK,MAAO,wBAAwB;AAC3C,OAAOC,UAAS,MAAO,0BAA0B;AACjD,OAAOC,SAAQ,MAAO,qBAAqB;AAE3C,eAAe;EACbC,IAAI,EAAE;IACJC,KAAK,EAAE,cAAc;IACrBC,IAAI,EAAE,CAAC;MACLC,IAAI,EAAE,aAAa;MACnBC,OAAO,EAAEL,SAAS,CAACM;IACrB,CAAC;EACH,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLL,KAAK,EAAE,cAAc;MACrBM,KAAK,EAAE,CAAC;QACNC,IAAI,EAAE,OAAO;QACbC,IAAI,EAAE;MACR,CAAC,EACD;QACED,IAAI,EAAE,cAAc;QACpBE,MAAM,EAAE;MACV,CAAC,CACA;MACDC,WAAW,EAAE,EAAE;MACfC,eAAe,EAAE,GAAG;MACpBC,eAAe,EAAE;QACfC,KAAK,EAAE,EAAE;QACTR,IAAI,EAAE,CAAC,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,