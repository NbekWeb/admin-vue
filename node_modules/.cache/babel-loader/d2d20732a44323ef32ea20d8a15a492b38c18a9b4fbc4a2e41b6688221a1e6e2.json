{"ast":null,"code":"import { ref, onMounted } from \"vue\";\nimport Layout from \"@/layouts/main.vue\";\nimport PageHeader from \"@/components/page-header.vue\";\nimport UserModal from \"./components/UserModal.vue\";\nimport { getUser_API } from \"@/service/user\";\nimport { Plus } from \"@element-plus/icons-vue\";\nexport default {\n  __name: 'basic',\n  setup(__props, {\n    expose\n  }) {\n    expose();\n    const title = \"Абоненты\";\n    const items = [{\n      text: \"Абоненты\",\n      href: \"/\",\n      active: true\n    }];\n    const subscriberRef = ref();\n    const subscribers = ref([]);\n    const loading = ref(false);\n    const fetchSubscribers = async () => {\n      loading.value = true;\n      const [error, res] = await getUser_API();\n      if (!error) {\n        subscribers.value = res.data.results;\n      } else {\n        console.error(\"Ошибка при получении абонентов:\", error);\n      }\n      loading.value = false;\n    };\n    const viewDetails = subscriber => {\n      alert(`Подробнее об абоненте: ${subscriber.name}`);\n    };\n    onMounted(() => {\n      fetchSubscribers();\n    });\n    const __returned__ = {\n      title,\n      items,\n      subscriberRef,\n      subscribers,\n      loading,\n      fetchSubscribers,\n      viewDetails,\n      ref,\n      onMounted,\n      Layout,\n      PageHeader,\n      UserModal,\n      get getUser_API() {\n        return getUser_API;\n      },\n      get Plus() {\n        return Plus;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMounted","Layout","PageHeader","UserModal","getUser_API","Plus","title","items","text","href","active","subscriberRef","subscribers","loading","fetchSubscribers","value","error","res","data","results","console","viewDetails","subscriber","alert","name"],"sources":["C:/Users/UMIDJON/Desktop/default2/src/views/tables/basic.vue"],"sourcesContent":["<template>\n  <Layout>\n    <SubscriberModal @create=\"fetchSubscribers\" ref=\"subscriberRef\" />\n    <PageHeader :title=\"title\" :items=\"items\" />\n\n    <el-card shadow=\"never\">\n      <div class=\"card-header mb-4\">\n        <span class=\"card-title\">Абоненты</span>\n        <el-button type=\"primary\" :icon=\"Plus\" @click=\"subscriberRef.open()\">\n          Добавить абонента\n        </el-button>\n      </div>\n\n      <div class=\"live-preview\">\n        <el-table\n          :data=\"subscribers\"\n          border\n          stripe\n          style=\"width: 100%\"\n          v-loading=\"loading\"\n          empty-text=\"Абоненты не найдены\"\n        >\n          <el-table-column prop=\"id\" label=\"ID\" width=\"80\" />\n          <el-table-column prop=\"name\" label=\"Имя\" />\n          <el-table-column prop=\"tariff_name\" label=\"Тариф\" />\n          <el-table-column prop=\"start_date\" label=\"Дата начала\" />\n          <el-table-column prop=\"end_date\" label=\"Дата окончание\" />\n          <el-table-column label=\"Статус\">\n            <template #default=\"{ row }\">\n              <el-tag :type=\"row.paid ? 'success' : 'danger'\">\n                {{ row.paid ? 'Оплачен' : 'Не оплачен' }}\n              </el-tag>\n            </template>\n          </el-table-column>\n          <el-table-column label=\"Подача воды\">\n            <template #default=\"{ row }\">\n              <el-switch v-model=\"row.water_supply\" disabled />\n            </template>\n          </el-table-column>\n          <el-table-column width=\"120\">\n            <template #default=\"{ row }\">\n              <el-button type=\"primary\" size=\"small\" @click=\"viewDetails(row)\">\n                Подробнее\n              </el-button>\n            </template>\n          </el-table-column>\n        </el-table>\n      </div>\n    </el-card>\n  </Layout>\n</template>\n\n<script setup>\nimport { ref, onMounted } from \"vue\";\nimport Layout from \"@/layouts/main.vue\";\nimport PageHeader from \"@/components/page-header.vue\";\nimport UserModal from \"./components/UserModal.vue\";\nimport { getUser_API } from \"@/service/user\";\nimport { Plus } from \"@element-plus/icons-vue\";\n\nconst title = \"Абоненты\";\nconst items = [{ text: \"Абоненты\", href: \"/\", active: true }];\n\nconst subscriberRef = ref();\nconst subscribers = ref([]);\nconst loading = ref(false);\n\nconst fetchSubscribers = async () => {\n  loading.value = true;\n  const [error, res] = await getUser_API();\n\n  if (!error) {\n    subscribers.value = res.data.results;\n  } else {\n    console.error(\"Ошибка при получении абонентов:\", error);\n  }\n\n  loading.value = false;\n};\n\nconst viewDetails = (subscriber) => {\n  alert(`Подробнее об абоненте: ${subscriber.name}`);\n};\n\nonMounted(() => {\n  fetchSubscribers();\n});\n</script>\n\n<style scoped>\n.card-header {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  font-weight: 600;\n  font-size: 18px;\n}\n</style>\n"],"mappings":"AAqDA,SAASA,GAAG,EAAEC,SAAS,QAAQ,KAAK;AACpC,OAAOC,MAAM,MAAM,oBAAoB;AACvC,OAAOC,UAAU,MAAM,8BAA8B;AACrD,OAAOC,SAAS,MAAM,4BAA4B;AAClD,SAASC,WAAW,QAAQ,gBAAgB;AAC5C,SAASC,IAAI,QAAQ,yBAAyB;;;;;;;IAE9C,MAAMC,KAAK,GAAG,UAAU;IACxB,MAAMC,KAAK,GAAG,CAAC;MAAEC,IAAI,EAAE,UAAU;MAAEC,IAAI,EAAE,GAAG;MAAEC,MAAM,EAAE;IAAK,CAAC,CAAC;IAE7D,MAAMC,aAAa,GAAGZ,GAAG,CAAC,CAAC;IAC3B,MAAMa,WAAW,GAAGb,GAAG,CAAC,EAAE,CAAC;IAC3B,MAAMc,OAAO,GAAGd,GAAG,CAAC,KAAK,CAAC;IAE1B,MAAMe,gBAAgB,GAAG,MAAAA,CAAA,KAAY;MACnCD,OAAO,CAACE,KAAK,GAAG,IAAI;MACpB,MAAM,CAACC,KAAK,EAAEC,GAAG,CAAC,GAAG,MAAMb,WAAW,CAAC,CAAC;MAExC,IAAI,CAACY,KAAK,EAAE;QACVJ,WAAW,CAACG,KAAK,GAAGE,GAAG,CAACC,IAAI,CAACC,OAAO;MACtC,CAAC,MAAM;QACLC,OAAO,CAACJ,KAAK,CAAC,iCAAiC,EAAEA,KAAK,CAAC;MACzD;MAEAH,OAAO,CAACE,KAAK,GAAG,KAAK;IACvB,CAAC;IAED,MAAMM,WAAW,GAAIC,UAAU,IAAK;MAClCC,KAAK,CAAC,0BAA0BD,UAAU,CAACE,IAAI,EAAE,CAAC;IACpD,CAAC;IAEDxB,SAAS,CAAC,MAAM;MACdc,gBAAgB,CAAC,CAAC;IACpB,CAAC,CAAC","ignor