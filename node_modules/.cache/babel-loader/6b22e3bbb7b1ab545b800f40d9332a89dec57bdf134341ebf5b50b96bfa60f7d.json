{"ast":null,"code":"import Layout from \"../../layouts/main.vue\";\nimport PageHeader from \"@/components/page-header\";\nimport 'prismjs';\nimport 'prismjs/themes/prism.css';\nexport default {\n  data() {\n    return {\n      title: \"Embed Video\",\n      items: [{\n        text: \"Base UI\",\n        href: \"/\"\n      }, {\n        text: \"Embed Video\",\n        active: true\n      }]\n    };\n  },\n  name: \"Embed Video\",\n  components: {\n    Layout,\n    PageHeader\n  },\n  mounted() {\n    const checkbox = document.getElementsByClassName(\"code-switcher\");\n    Array.from(checkbox).forEach(check => {\n      check.addEventListener('change', () => {\n        const card = check.closest('.card');\n        const preview = card.querySelector(\".live-preview\");\n        const code = card.querySelector(\".code-view\");\n        if (check.checked) {\n          // do this\n          preview.classList.add(\"d-none\");\n          code.classList.remove(\"d-none\");\n        } else {\n          // do that\n          preview.classList.remove(\"d-none\");\n          code.classList.add(\"d-none\");\n        }\n      });\n    });\n  }\n};","map":{"version":3,"names":["Layout","PageHeader","data","title","items","text","href","active","name","components","mounted","checkbox","document","getElementsByClassName","Array","from","forEach","check","addEventListener","card","closest","preview","querySelector","code","checked","classList","add","remove"],"sources":["C:\\Users\\UMIDJON\\Desktop\\default2\\src\\views\\ui\\embed-video.vue"],"sourcesContent":["<script>\nimport Layout from \"../../layouts/main.vue\";\nimport PageHeader from \"@/components/page-header\";\n\nimport 'prismjs';\nimport 'prismjs/themes/prism.css';\n\nexport default {\n    data() {\n        return {\n            title: \"Embed Video\",\n            items: [\n                {\n                    text: \"Base UI\",\n                    href: \"/\",\n                },\n                {\n                    text: \"Embed Video\",\n                    active: true,\n                },\n            ],\n        };\n    },\n    name: \"Embed Video\",\n    components: {\n        Layout,\n        PageHeader,\n    },\n    mounted() {\n        const checkbox = document.getElementsByClassName(\"code-switcher\");\n        Array.from(checkbox).forEach((check) => {\n            check.addEventListener('change', () => {\n                const card = check.closest('.card');\n                const preview = card.querySelector(\".live-preview\");\n                const code = card.querySelector(\".code-view\");\n                if (check.checked) {\n                    // do this\n                    preview.classList.add(\"d-none\");\n                    code.classList.remove(\"d-none\");\n                } else {\n                    // do that\n                    preview.classList.remove(\"d-none\");\n                    code.classList.add(\"d-none\");\n                }\n            });\n        });\n    }\n\n};\n</script>\n\n<template>\n    <Layout>\n        <PageHeader :title=\"title\" :items=\"items\" />\n        <b-row>\n            <b-col xl=\"6\">\n                <b-card no-body>\n                    <b-card-header class=\"align-items-center d-flex\">\n                        <b-card-title class=\"mb-0 flex-grow-1\">Ratio Video 16:9</b-card-title>\n                        <div class=\"flex-shrink-0\">\n                            <div class=\"form-check form-switch form-switch-right form-switch-md\">\n                                <label for=\"ratiovideo1-showcode\" class=\"form-label text-muted\">Show Code</label>\n                                <input class=\"form-check-input code-switcher\" type=\"checkbox\"\n                                    id=\"ratiovideo1-showcode\" />\n                            </div>\n                        </div>\n                    </b-card-header>\n                    <b-card-body>\n                        <p class=\"text-muted\">\n                            Wrap any embed in\n                            <code>&lt;iframe&gt;</code> tag, in a parent element, use\n                            <code>ratio-16x9</code> class to set aspect ratio 16:9.\n                        </p>\n                        <div class=\"live-preview\">\n                            <!-- 16:9 aspect ratio -->\n                            <div class=\"ratio ratio-16x9\">\n                                <iframe class=\"rounded\" src=\"https://www.youtube.com/embed/1y_kfWUCFDQ\"\n                                    title=\"YouTube video\" allowfullscreen></iframe>\n                            </div>\n                        </div>\n                        <div class=\"d-none code-view\">\n                            <pre class=\"language-markup\">\n<code>&lt;!-- Ratio Video 16:9 --&gt;\n&lt;div class=&quot;ratio ratio-16x9&quot;&gt;\n&lt;iframe src=&quot;https://www.youtube.com/embed/1y_kfWUCFDQ&quot; title=&quot;YouTube video&quot; allowfullscreen&gt;&lt;/iframe&gt;\n&lt;/div&gt;</code></pre>\n                        </div>\n                    </b-card-body>\n                </b-card>\n\n                <b-card no-body>\n                    <b-card-header class=\"align-items-center d-flex\">\n                        <b-card-title class=\"mb-0 flex-grow-1\">Ratio Video 4:3</b-card-title>\n                        <div class=\"flex-shrink-0\">\n                            <div class=\"form-check form-switch form-switch-right form-switch-md\">\n                                <label for=\"ratiovideo3-showcode\" class=\"form-label text-muted\">Show Code</label>\n                                <input class=\"form-check-input code-switcher\" type=\"checkbox\"\n                                    id=\"ratiovideo3-showcode\" />\n                            </div>\n                        </div>\n                    </b-card-header>\n                    <b-card-body>\n                        <p class=\"text-muted\">\n                            Use\n                            <code>ratio-4x3</code> class to set aspect ratio 4:3.\n                        </p>\n                        <div class=\"live-preview\">\n                            <!-- 4:3 aspect ratio -->\n                            <div class=\"ratio ratio-4x3\">\n                                <iframe class=\"rounded\" src=\"https://www.youtube.com/embed/PHcgN1GTjdU\"\n                                    title=\"YouTube video\" allowfullscreen></iframe>\n                            </div>\n                        </div>\n                        <div class=\"d-none code-view\">\n                            <pre class=\"language-markup\">\n<code>&lt;!-- Ratio Video 4:3 --&gt;\n&lt;div class=&quot;ratio ratio-4x3&quot;&gt;\n&lt;iframe src=&quot;https://www.youtube.com/embed/1y_kfWUCFDQ&quot; title=&quot;YouTube video&quot; allowfullscreen&gt;&lt;/iframe&gt;\n&lt;/div&gt;</code></pre>\n                        </div>\n                    </b-card-body>\n                </b-card>\n\n                <b-card no-body>\n                    <b-card-header class=\"align-items-center d-flex\">\n                        <b-card-title class=\"mb-0 flex-grow-1\">Custom Ratios</b-card-title>\n                        <div class=\"flex-shrink-0\">\n                            <div class=\"form-check form-switch form-switch-right form-switch-md\">\n                                <label for=\"ratiovideocustom-showcode\" class=\"form-label text-muted\">Show Code</label>\n                                <input class=\"form-check-input code-switcher\" type=\"checkbox\"\n                                    id=\"ratiovideocustom-showcode\" />\n                            </div>\n                        </div>\n                    </b-card-header>\n                    <b-card-body>\n                        <p class=\"text-muted\">\n                            Use\n                            <code>--vz-aspect-ratio: 50%</code> to style element to set aspect ratio 2:1.\n                        </p>\n                        <div class=\"live-preview\">\n                            <!-- 16:9 aspect ratio -->\n                            <div class=\"ratio\" style=\"--vz-aspect-ratio: 50%;\">\n                                <iframe class=\"rounded\" src=\"https://www.youtube.com/embed/2RZQN_ko0iU\"\n                                    title=\"YouTube video\" allowfullscreen></iframe>\n                            </div>\n                        </div>\n                        <div class=\"d-none code-view\">\n                            <pre class=\"language-markup\">\n<code>&lt;!-- Custom Ratio Video --&gt;\n&lt;div class=&quot;ratio&quot; style=&quot;--vz-aspect-ratio: 50%;&quot;&gt;\n&lt;iframe src=&quot;https://www.youtube.com/embed/2RZQN_ko0iU&quot; title=&quot;YouTube video&quot; allowfullscreen&gt;&lt;/iframe&gt;\n&lt;/div&gt;</code></pre>\n                        </div>\n                    </b-card-body>\n                </b-card>\n            </b-col>\n\n            <b-col xl=\"6\">\n                <b-card no-body>\n                    <b-card-header class=\"align-items-center d-flex\">\n                        <b-card-title class=\"mb-0 flex-grow-1\">Ratio Video 21:9</b-card-title>\n                        <div class=\"flex-shrink-0\">\n                            <div class=\"form-check form-switch form-switch-right form-switch-md\">\n                                <label for=\"ratiovideo2-showcode\" class=\"form-label text-muted\">Show Code</label>\n                                <input class=\"form-check-input code-switcher\" type=\"checkbox\"\n                                    id=\"ratiovideo2-showcode\" />\n                            </div>\n                        </div>\n                    </b-card-header>\n                    <b-card-body>\n                        <p class=\"text-muted\">\n                            Use\n                            <code>ratio-21x9</code> class to set aspect ratio 21:9.\n                        </p>\n                        <div class=\"live-preview\">\n                            <!-- 21:9 aspect ratio -->\n                            <div class=\"ratio ratio-21x9\">\n                                <iframe class=\"rounded\" src=\"https://www.youtube.com/embed/Z-fV2lGKnnU\"\n                                    title=\"YouTube video\" allowfullscreen></iframe>\n                            </div>\n                        </div>\n                        <div class=\"d-none code-view\">\n                            <pre class=\"language-markup\">\n<code>&lt;!-- Ratio Video 21:9 --&gt;\n&lt;div class=&quot;ratio ratio-21x9&quot;&gt;\n&lt;iframe src=&quot;https://www.youtube.com/embed/Z-fV2lGKnnU&quot; title=&quot;YouTube video&quot; allowfullscreen&gt;&lt;/iframe&gt;\n&lt;/div&gt;</code></pre>\n                        </div>\n                    </b-card-body>\n                </b-card>\n\n                <b-card no-body>\n                    <b-card-header class=\"align-items-center d-flex\">\n                        <b-card-title class=\"mb-0 flex-grow-1\">Ratio Video 1:1</b-card-title>\n                        <div class=\"flex-shrink-0\">\n                            <div class=\"form-check form-switch form-switch-right form-switch-md\">\n                                <label for=\"ratiovideo4-showcode\" class=\"form-label text-muted\">Show Code</label>\n                                <input class=\"form-check-input code-switcher\" type=\"checkbox\"\n                                    id=\"ratiovideo4-showcode\" />\n                            </div>\n                        </div>\n                    </b-card-header>\n                    <b-card-body>\n                        <p class=\"text-muted\">\n                            Use\n                            <code>ratio-1x1</code> class to set aspect ratio 1:1.\n                        </p>\n                        <div class=\"live-preview\">\n                            <!-- 1:1 aspect ratio -->\n                            <div class=\"ratio ratio-1x1\">\n                                <iframe class=\"rounded\" src=\"https://www.youtube.com/embed/GfSZtaoc5bw\"\n                                    title=\"YouTube video\" allowfullscreen></iframe>\n                            </div>\n                        </div>\n                        <div class=\"d-none code-view\">\n                            <pre class=\"language-markup\">\n<code>&lt;!-- Ratio Video 1:1 --&gt;\n&lt;div class=&quot;ratio ratio-1x1&quot;&gt;\n&lt;iframe src=&quot;https://www.youtube.com/embed/GfSZtaoc5bw&quot; title=&quot;YouTube video&quot; allowfullscreen&gt;&lt;/iframe&gt;\n&lt;/div&gt;</code></pre>\n                        </div>\n                    </b-card-body>\n                </b-card>\n            </b-col>\n        </b-row>\n    </Layout>\n</template>"],"mappings":"AACA,OAAOA,MAAK,MAAO,wBAAwB;AAC3C,OAAOC,UAAS,MAAO,0BAA0B;AAEjD,OAAO,SAAS;AAChB,OAAO,0BAA0B;AAEjC,eAAe;EACXC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,KAAK,EAAE,aAAa;MACpBC,KAAK,EAAE,CACH;QACIC,IAAI,EAAE,SAAS;QACfC,IAAI,EAAE;MACV,CAAC,EACD;QACID,IAAI,EAAE,aAAa;QACnBE,MAAM,EAAE;MACZ,CAAC;IAET,CAAC;EACL,CAAC;EACDC,IAAI,EAAE,aAAa;EACnBC,UAAU,EAAE;IACRT,MAAM;IACNC;EACJ,CAAC;EACDS,OAAOA,CAAA,EAAG;IACN,MAAMC,QAAO,GAAIC,QAAQ,CAACC,sBAAsB,CAAC,eAAe,CAAC;IACjEC,KAAK,CAACC,IAAI,CAACJ,QAAQ,CAAC,CAACK,OAAO,CAAEC,KAAK,IAAK;MACpCA,KAAK,CAACC,gBAAgB,CAAC,QAAQ,EAAE,MAAM;QACnC,MAAMC,IAAG,GAAIF,KAAK,CAACG,OAAO,CAAC,OAAO,CAAC;QACnC,MAAMC,OAAM,GAAIF,IAAI,CAACG,aAAa,CAAC,eAAe,CAAC;QACnD,MAAMC,IAAG,GAAIJ,IAAI,CAACG,aAAa,CAAC,YAAY,CAAC;QAC7C,IAAIL,KAAK,CAACO,OAAO,EAAE;UACf;UACAH,OAAO,CAACI,SAAS,CAACC,GAAG,CAAC,QAAQ,CAAC;UAC/BH,IAAI,CAACE,SAAS,CAACE,MAAM,CAAC,QAAQ,CAAC;QACnC,OAAO;UACH;UACAN,OAAO,CAACI,SAAS,CAACE,MAAM,CAAC,QAAQ,CAAC;UAClCJ,IAAI,CAACE,SAAS,CAACC,GAAG,CAAC,QAAQ,CAAC;QAChC;MACJ,CAAC,CAAC;IACN,CAAC,CAAC;EACN;AAEJ,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}