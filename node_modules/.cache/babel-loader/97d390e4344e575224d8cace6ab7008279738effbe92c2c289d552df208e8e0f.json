{"ast":null,"code":"import Layout from \"../../../layouts/main.vue\";\nimport appConfig from \"../../../../app.config\";\nimport PageHeader from \"@/components/page-header\";\nimport animationData from \"@/components/widgets/uetqnvvg.json\";\nimport Lottie from \"@/components/widgets/lottie.vue\";\nexport default {\n  page: {\n    title: \"Order Details\",\n    meta: [{\n      name: \"description\",\n      content: appConfig.description\n    }]\n  },\n  data() {\n    return {\n      title: \"Order Details\",\n      items: [{\n        text: \"Ecommerce\",\n        href: \"/\"\n      }, {\n        text: \"Order Details\",\n        active: true\n      }],\n      orderSummary: [{\n        id: 1,\n        img: require(\"@/assets/images/products/img-8.png\"),\n        product: \"Sweatshirt for Men (Pink)\",\n        price: 119.99,\n        quantity: 2,\n        size: \"M\",\n        color: \"Pink\"\n      }, {\n        id: 2,\n        img: require(\"@/assets/images/products/img-7.png\"),\n        product: \"Noise NoiseFit Endure Smart Watch\",\n        price: 94.99,\n        quantity: 1,\n        size: \"32.5mm\",\n        color: \"Black\"\n      }, {\n        id: 3,\n        img: require(\"@/assets/images/products/img-3.png\"),\n        product: \"350 ml Glass Grocery Container\",\n        price: 24.99,\n        quantity: 1,\n        size: \"350 ml\",\n        color: \"White\"\n      }],\n      defaultOptions: {\n        animationData: animationData\n      }\n    };\n  },\n  components: {\n    Layout,\n    PageHeader,\n    lottie: Lottie\n  }\n};","map":{"version":3,"names":["Layout","appConfig","PageHeader","animationData","Lottie","page","title","meta","name","content","description","data","items","text","href","active","orderSummary","id","img","require","product","price","quantity","size","color","defaultOptions","components","lottie"],"sources":["C:\\Users\\UMIDJON\\Desktop\\default2\\src\\views\\apps\\ecommerce\\order-details.vue"],"sourcesContent":["<script>\nimpo