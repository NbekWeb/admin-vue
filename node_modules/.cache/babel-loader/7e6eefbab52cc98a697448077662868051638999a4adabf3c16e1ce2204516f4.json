{"ast":null,"code":"import Layout from \"../../layouts/main.vue\";\nimport PageHeader from \"@/components/page-header\";\nimport 'prismjs';\nimport 'prismjs/themes/prism.css';\nexport default {\n  data() {\n    return {\n      title: \"Grid\",\n      items: [{\n        text: \"Base UI\",\n        href: \"/\"\n      }, {\n        text: \"Grid\",\n        active: true\n      }]\n    };\n  },\n  name: \"Grid\",\n  components: {\n    Layout,\n    PageHeader\n  },\n  mounted() {\n    const checkbox = document.getElementsByClassName(\"code-switcher\");\n    Array.from(checkbox).forEach(check => {\n      check.addEventListener('change', () => {\n        const card = check.closest('.card');\n        const preview = card.querySelector(\".live-preview\");\n        const code = card.querySelector(\".code-view\");\n        if (check.checked) {\n          // do this\n          preview.classList.add(\"d-none\");\n          code.classList.remove(\"d-none\");\n        } else {\n          // do that\n          preview.classList.remove(\"d-none\");\n          code.classList.add(\"d-none\");\n        }\n      });\n    });\n  }\n};","map":{"version":3,"names":["Layout","PageHeader","data","title","items","text","href","active","name","components","mounted","checkbox","document","getElementsByClassName","Array","from","forEach","check","addEventListener","card","closest","preview","querySelector","code","checked","classList","add","remove"],"sources":["C:\\Users\\UMIDJON\\Desktop\\default2\\src\\views\\ui\\grid.vue"],"sourcesContent":["<script>\nimport Layout from \"../../layouts/main.vue\";\nimport PageHeader from \"@/components/page-header\";\n\nimport 'prismjs';\nimport 'prismjs/themes/prism.css';\n\nexport default {\n    data() {\n        return {\n            title: \"Grid\",\n            items: [\n                {\n                    text: \"Base UI\",\n                    href: \"/\",\n                },\n                {\n                    text: \"Grid\",\n                    active: true,\n                },\n            ],\n        };\n    },\n    name: \"Grid\",\n    components: {\n        Layout,\n        PageHeader,\n    },\n    mounted() {\n        const checkbox = document.getElementsByClassName(\"code-switcher\");\n        Array.from(checkbox).forEach((check) => {\n            check.addEventListener('change', () => {\n                const card = check.closest('.card');\n                const preview = card.querySelector(\".live-preview\");\n                const code = card.querySelector(\".code-view\");\n                if (check.checked) {\n                    // do this\n                    preview.classList.add(\"d-none\");\n                    code.classList.remove(\"d-none\");\n                } else {\n                    // do that\n                    preview.classList.remove(\"d-none\");\n                    code.classList.add(\"d-none\");\n                }\n            });\n        });\n    }\n};\n</script>\n\n<template>\n    <Layout>\n        <PageHeader :title=\"title\" :items=\"items\" />\n        <b-row>\n            <b-col cols=\"12\">\n                <b-card no-body>\n                    <b-card-header>\n                        <b-card-title class=\"mb-0\">Grid Options</b-card-title>\n                    </b-card-header>\n\n                    <b-card-body>\n                        <p class=\"text-muted\">\n                            See how aspects of the Bootstrap grid\n                            system work across multiple devices with a handy table.\n                        </p>\n                        <div class=\"table-responsive\">\n                            <table class=\"table table-bordered table-nowrap mb-0\">\n                                <thead>\n                                    <tr>\n                                        <th scope=\"col\"></th>\n                                        <th scope=\"col\" class=\"text-center\">\n                                            xs\n       