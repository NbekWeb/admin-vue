{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { ref, watch } from \"vue\";\nimport useVuelidate from \"@vuelidate/core\";\nimport DropZone from \"@/components/widgets/dropZone\";\nimport Layout from \"../../layouts/main.vue\";\nimport PageHeader from \"@/components/page-header\";\nimport appConfig from \"../../../app.config\";\nexport default {\n  methods: {\n    deleteRecord(ele) {\n      ele.target.parentElement.parentElement.parentElement.remove();\n    }\n  },\n  page: {\n    title: \"File Uploads\",\n    meta: [{\n      name: \"description\",\n      content: appConfig.description\n    }]\n  },\n  setup() {\n    let files = ref([]);\n    let dropzoneFile = ref(\"\");\n    const drop = e => {\n      dropzoneFile.value = e.dataTransfer.files[0];\n      files.value.push(dropzoneFile.value);\n    };\n    const selectedFile = () => {\n      dropzoneFile.value = document.querySelector(\".dropzoneFile\").files[0];\n      files.value.push(dropzoneFile.value);\n    };\n    watch(() => [...files.value], currentValue => {\n      return currentValue;\n    });\n    return {\n      dropzoneFile,\n      files,\n      drop,\n      selectedFile,\n      v$: useVuelidate()\n    };\n  },\n  data() {\n    return {\n      title: \"File Uploads\",\n      items: [{\n        text: \"Forms\",\n        href: \"/\"\n      }, {\n        text: \"File Uploads\",\n        active: true\n      }]\n    };\n  },\n  components: {\n    Layout,\n    PageHeader,\n    DropZone\n  }\n};","map":{"version":3,"names":["ref","watch","useVuelidate","DropZone","Layout","PageHeader","appConfig","methods","deleteRecord","ele","target","parentElement","remove","page","title","meta","name","content","description","setup","files","dropzoneFile","drop","e","value","dataTransfer","push","selectedFile","document","querySelector","currentValue","v$","data","items","text","href","active","components"],"sources":["C:\\Users\\UMIDJON\\Desktop\\default2\\src\\views\\forms\\file-uploads.vue"],"sourcesContent":["<script>\nimport { ref, watch } from \"vue\";\nimport useVuelidate from \"@vuelidate/core\";\n\nimport DropZone from \"@/components/widgets/dropZone\";\n\nimport Layout from \"../../layouts/main.vue\";\nimport PageHeader from \"@/components/page-header\";\nimport appConfig from \"../../../app.config\";\n\nexport default {\n  methods: {\n    deleteRecord(ele) {\n      ele.target.parentElement.parentElement.parentElement.remove();\n    },\n  },\n  page: {\n    title: \"File Uploads\",\n    meta: [{ name: \"description\", content: appConfig.description }],\n  },\n  setup() {\n    let files = ref([]);\n    let dropzoneFile = ref(\"\");\n    const drop = (e) => {\n      dropzoneFile.value = e.dataTransfer.files[0];\n      files.value.push(dropzoneFile.value);\n    };\n    const selectedFile = () => {\n      dropzoneFile.value = document.querySelector(\".dropzoneFile\").files[0];\n      files.value.push(dropzoneFile.value);\n    };\n    watch(\n      () => [...files.value],\n      (currentValue) => {\n        return currentValue;\n      }\n    );\n\n    return { dropzoneFile, files, drop, selectedFile, v$: useVuelidate() };\n  },\n  data() {\n    return {\n      title: \"File Uploads\",\n      items: [\n        {\n          text: \"Forms\",\n          href: \"/\",\n        },\n        {\n          text: \"File Uploads\",\n          active: true,\n        },\n      ],\n    };\n  },\n  components: {\n    Layout,\n    PageHeader,\n    DropZone,\n  },\n};\n</script>\n\n<template>\n  <Layout>\n    <PageHeader :title=\"title\" :items=\"items\" />\n    <b-row>\n      <b-col lg=\"12\">\n        <b-card no-body>\n          <b-card-header>\n            <b-card-title class=\"mb-0\">Dropzone</b-card-title>\n          </b-card-header>\n\n          <b-card-body>\n            <p class=\"text-muted\">\n              DropzoneJS is an open source library that provides drag’n’drop\n              file uploads with image previews.\n            </p>\n\n            <DropZone @drop.prevent=\"drop\" @change=\"selectedFile\" />\n            <ul class=\"list-unstyled mb-0\" id=\"dropzone-preview\">\n              <div class=\"border rounded\" v-for=\"(file, index) of files\" :key=\"index\">\n                <div class=\"d-flex p-2\">\n                  <div class=\"flex-grow-1\">\n                    <div class=\"pt-1\">\n                      <h5 class=\"fs-14 mb-1\" data-dz-name=\"\">\n                        {{ file.name }}\n                      </h5>\n                      <p class=\"fs-13 text-muted mb-0\" data-dz-size=\"\">\n                        <strong>{{ file.size / 1024 }}</strong> KB\n                      </p>\n                      <strong class=\"error text-danger\" data-dz-errormessage=\"\"></strong>\n                    </div>\n                  </div>\n                  <div class=\"flex-shrink-0 ms-3\">\n                    <b-button variant=\"danger\" size=\"sm\" data-dz-remove=\"\" @click=\"deleteRecord\">\n                      Delete\n                    </b-button>\n                  </div>\n                </div>\n              </div>\n            </ul>\n          </b-card-body>\n        </b-card>\n      </b-col>\n    </b-row>\n\n  </Layout>\n</template>\n"],"mappings":";AACA,SAASA,GAAG,EAAEC,KAAI,QAAS,KAAK;AAChC,OAAOC,YAAW,MAAO,iBAAiB;AAE1C,OAAOC,QAAO,MAAO,+BAA+B;AAEpD,OAAOC,MAAK,MAAO,wBAAwB;AAC3C,OAAOC,UAAS,MAAO,0BAA0B;AACjD,OAAOC,SAAQ,MAAO,qBAAqB;AAE3C,eAAe;EACbC,OAAO,EAAE;IACPC,YAAYA,CAACC,GAAG,EAAE;MAChBA,GAAG,CAACC,MAAM,CAACC,aAAa,CAACA,aAAa,CAACA,aAAa,CAACC,MAAM,CAAC,CAAC;IAC/D;EACF,CAAC;EACDC,IAAI,EAAE;IACJC,KAAK,EAAE,cAAc;IACrBC,IAAI,EAAE,CAAC;MAAEC,IAAI,EAAE,aAAa;MAAEC,OAAO,EAAEX,SAAS,CAACY;IAAY,CAAC;EAChE,CAAC;EACDC,KAAKA,CAAA,EAAG;IACN,IAAIC,KAAI,GAAIpB,GAAG,CAAC,EAAE,CAAC;IACnB,IAAIqB,YAAW,GAAIrB,GAAG,CAAC,EAAE,CAAC;IAC1B,MAAMsB,IAAG,GAAKC,CAAC,IAAK;MAClBF,YAAY,CAACG,KAAI,GAAID,CAAC,CAACE,YAAY,CAACL,KAAK,CAAC,CAAC,CAAC;MAC5CA,KAAK,CAACI,KAAK,CAACE,IAAI,CAACL,YAAY,CAACG,KAAK,CAAC;IACtC,CAAC;IACD,MAAMG,YAA